Fragment","_renderList","producto","id_producto","_hoisted_3","_toDisplayString","descripcion","precio_unitario","_hoisted_4","onClick","$event","agregarCarrito","_hoisted_6","_createTextVNode","_hoisted_5","_hoisted_7","_hoisted_9","_cache","args","toggleSidebar","_hoisted_10","_normalizeClass","$data","sidebarAbierto","_hoisted_12","_hoisted_13","carrito","item","index","id_carrito_producto","cantidad","eliminarDelCarrito","_hoisted_14","totalCarrito","_hoisted_15","_hoisted_16","_createVNode","_component_b_form_select","modelValue","metodo_pago_id","default","_withCtx","_component_b_form_select_option","value","_","procesarVenta"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ProductosTienda.vue"],"sourcesContent":["<template>\n  <div class=\"productos-tienda\">\n    <!-- Contenedor de productos -->\n    <div v-if=\"productosFiltrados.length > 0\" class=\"productos-grid\">\n      <div\n        v-for=\"producto in productosFiltrados\"\n        :key=\"producto.id_producto\"\n        class=\"producto-card\"\n      >\n        <div class=\"producto-descripcion\">\n          <h3>{{ producto.descripcion }}</h3>\n          <p>Precio: ${{ producto.precio_unitario }}</p>\n        </div>\n        <div class=\"producto-acciones\">\n          <button @click=\"agregarCarrito(producto)\" class=\"btn-agregar\">\n            <i class=\"fas fa-shopping-cart icono-carrito\"></i> Agregar al\n            carrito\n          </button>\n        </div>\n      </div>\n    </div>\n    <div v-else>\n      <p>No hay productos disponibles.</p>\n    </div>\n\n    <button @click=\"toggleSidebar\" class=\"btn-sidebar\">\n      <i class=\"fas fa-shopping-cart icono-carrito\"></i> Ver carrito\n    </button>\n\n    <div :class=\"['sidebar', { 'sidebar-abierto': sidebarAbierto }]\">\n      <button @click=\"toggleSidebar\" class=\"btn-cerrar\"><i class=\"fa-solid fa-times\"></i>\n      </button>\n      <h3>Carrito de compras</h3>\n      <ul>\n        <li v-for=\"(item, index) in carrito\" :key=\"item.id_carrito_producto\">\n          {{ item.descripcion }} - ${{ item.precio_unitario }} x\n          {{ item.cantidad }}\n          <button class=\"btn-eliminar\" @click=\"eliminarDelCarrito(index)\">\n            Eliminar\n          </button>\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n      <div class=\"formulario-venta\">\n        <h4>Selecciona el método de pago</h4>\n        <b-form-select v-model=\"metodo_pago_id\" class=\"form-select\">\n          <b-form-select-option value=\"1\"\n            >Tarjeta de Crédito</b-form-select-option\n          >\n          <b-form-select-option value=\"2\">Mercadopago</b-form-select-option>\n          <b-form-select-option value=\"3\">Rapipago</b-form-select-option>\n        </b-form-select>\n        <button class=\"btn-procesar\" @click=\"procesarVenta\">\n          Finalizar compra\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  props: {\n    categoriaSeleccionada: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      productos: [],\n      carrito: [],\n      totalCarrito: 0,\n      id_cliente: null,\n      sidebarAbierto: false,\n      metodo_pago_id: 1,\n    };\n  },\n  computed: {\n    productosFiltrados() {\n      if (this.categoriaSeleccionada) {\n        return this.productos.filter(\n          (producto) =>\n            producto.nombre_subcategoria === this.categoriaSeleccionada\n        );\n      }\n      return this.productos;\n    },\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarAbierto = !this.sidebarAbierto;\n    },\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pro\", {\n          params: {\n            categoria: this.categoriaSeleccionada, // Enviar la categoría seleccionada como parámetro de consulta\n          },\n        });\n        if (Array.isArray(response.data)) {\n          this.productos = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n    async fetchMetodosPago() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/metodos_pago\"\n        );\n        if (Array.isArray(response.data)) {\n          this.metodosPago = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener métodos de pago:\", error);\n      }\n    },\n    async agregarCarrito(producto) {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      if (!this.id_cliente) {\n        alert(\"El cliente no está definido.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/carrito\",\n          {\n            id_cliente: this.id_cliente,\n            id_producto: producto.id_producto,\n            cantidad: 1,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`, // Aquí se corrige la sintaxis\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto agregado al carrito\") {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n        } else {\n          alert(\"Error al agregar el producto al carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al agregar al carrito:\", error);\n        alert(\"Error al agregar el producto al carrito.\");\n      }\n    },\n\n    async eliminarDelCarrito(index) {\n      const token = localStorage.getItem(\"token\");\n      const producto = this.carrito[index];\n\n      try {\n        const response = await axios.delete(\n          `http://localhost:3001/api/auth/carrito/${producto.id_carrito_producto}`, // Aquí se corrige la sintaxis\n          {\n            headers: {\n              Authorization: `Bearer ${token}`, // Aquí se corrige la sintaxis\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto eliminado del carrito\") {\n          this.carrito.splice(index, 1);\n          this.actualizarTotal();\n        } else {\n          alert(\"Error al eliminar el producto del carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al eliminar del carrito:\", error);\n        alert(\"Error al eliminar el producto del carrito.\");\n      }\n    },\n    actualizarTotal() {\n      this.totalCarrito = this.carrito.reduce((total, item) => {\n        return total + item.precio_unitario * item.cantidad;\n      }, 0);\n    },\n    async procesarVenta() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !id_cliente) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/venta\",\n          {\n            id_cliente,\n            productos: this.carrito,\n            total: this.totalCarrito,\n            metodo_pago_id: this.metodo_pago_id,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`, // Aquí se corrige la sintaxis\n            },\n          }\n        );\n\n        if (response.data.message === \"Venta registrada exitosamente\") {\n          alert(\"Compra realizada exitosamente!\");\n          this.carrito = [];\n          this.totalCarrito = 0;\n          this.metodo_pago_id = 1;\n        } else {\n          alert(\"Error al procesar la compra.\");\n        }\n      } catch (error) {\n        console.error(\"Error al procesar la compra:\", error);\n        alert(\"Error al procesar la compra.\");\n      }\n    },\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchMetodosPago();\n  },\n};\n</script>\n\n<style scoped>\n.productos-tienda {\n  padding: 20px;\n  background-color: #f5f5f5;\n  position: relative;\n}\n\n.productos-grid {\n  font-weight: bold;\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n  justify-content: center;\n}\n\n.producto-card {\n  border: 1px solid #ddd;\n  padding: 20px;\n  width: 250px;\n  text-align: center;\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.2s;\n  position: relative;\n}\n\n.producto-card:hover {\n  transform: scale(1.05);\n}\n\n.producto-descripcion h3 {\n  font-weight: bold;\n  margin: 0;\n  font-size: 20px;\n  color: #333;\n}\n\n.producto-descripcion p {\n  font-size: 18px;\n  color: #555;\n}\n\n.producto-acciones {\n  margin-top: 15px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.producto-acciones button {\n  margin: 5px;\n  padding: 10px 20px;\n  background-color: #0a641a;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  font-weight: bold;\n  transition: background-color 0.3s;\n  display: flex;\n  align-items: center;\n}\n\n.producto-acciones button:hover {\n  background-color: #09831f;\n}\n\n.icono-carrito {\n  color: #f1c40f;\n}\n\n.btn-sidebar {\n  position: fixed;\n  right: 20px;\n  bottom: 20px;\n  background-color: #0a641a;\n  color: white;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n  border: none;\n  font-size: 16px;\n}\n\n.sidebar {\n  position: fixed;\n  right: -350px;\n  top: 0;\n  height: 100%;\n  width: 350px;\n  background-color: #0a641a;\n  box-shadow: -4px 0 6px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  transition: right 0.3s;\n  z-index: 1000;\n}\n\n.sidebar-abierto {\n  right: 0;\n}\nh3{\n  \n}\n\n.btn-cerrar {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  z-index: 1001;\n  background-color: transparent;\n  border: none;\n  font-size: 24px;\n  color: #ff9800;\n}\n\n.formulario-venta {\n  color: #fff;\n  margin-top: 20px;\n}\n\n.formulario-venta select {\n  width: 100%;\n  padding: 10px;\n  margin-bottom: 10px;\n}\n\n.btn-procesar {\n  width: 100%;\n  padding: 10px;\n  background-color: #0a641a;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  font-weight: bold;\n}\n\n.btn-procesar:hover {\n  background-color: #09831f;\n}\n\n.btn-eliminar {\n  margin-left: 10px;\n  background-color: #e74c3c;\n  color: white;\n  padding: 5px 10px;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n}\n\n.btn-eliminar:hover {\n  background-color: #c0392b;\n}\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAkB;;EAD/BC,GAAA;EAG8CD,KAAK,EAAC;;;EAMvCA,KAAK,EAAC;AAAsB;;EAI5BA,KAAK,EAAC;AAAmB;mBAbtC;gEAeYE,mBAAA,CAAkD;EAA/CF,KAAK,EAAC;AAAoC;;EAfzDC,GAAA;AAAA;gEAsBMC,mBAAA,CAAoC,WAAjC,+BAA6B;oBAAhCC,UAAoC,C;iEAIpCD,mBAAA,CAAkD;EAA/CF,KAAK,EAAC;AAAoC;iEAIKE,mBAAA,CAAiC;EAA9BF,KAAK,EAAC;AAAmB;qBAA5BI,WAAiC,C;iEAEnFF,mBAAA,CAA2B,YAAvB,oBAAkB;oBAhC5B;;EA2CWF,KAAK,EAAC;AAAkB;iEAC3BE,mBAAA,CAAqC,YAAjC,8BAA4B;;;;uBA3CtCG,mBAAA,CAwDM,OAxDNC,UAwDM,GAvDJC,mBAAA,6BAAgC,EACrBC,QAAA,CAAAC,kBAAkB,CAACC,MAAM,Q,cAApCL,mBAAA,CAiBM,OAjBNM,UAiBM,I,kBAhBJN,mBAAA,CAeMO,SAAA,QAnBZC,WAAA,CAK2BL,QAAA,CAAAC,kBAAkB,EAA9BK,QAAQ;yBADjBT,mBAAA,CAeM;MAbHJ,GAAG,EAAEa,QAAQ,CAACC,WAAW;MAC1Bf,KAAK,EAAC;QAENE,mBAAA,CAGM,OAHNc,UAGM,GAFJd,mBAAA,CAAmC,YAAAe,gBAAA,CAA5BH,QAAQ,CAACI,WAAW,kBAC3BhB,mBAAA,CAA8C,WAA3C,WAAS,GAAAe,gBAAA,CAAGH,QAAQ,CAACK,eAAe,iB,GAEzCjB,mBAAA,CAKM,OALNkB,UAKM,GAJJlB,mBAAA,CAGS;MAHAmB,OAAK,EAAAC,MAAA,IAAEd,QAAA,CAAAe,cAAc,CAACT,QAAQ;MAAGd,KAAK,EAAC;QAC9CwB,UAAkD,EAf9DC,gBAAA,CAe8D,sBAEpD,E,iBAjBVC,UAAA,E;qDAqBIrB,mBAAA,CAEM,OAvBVsB,UAAA,MAAAC,UAAA,KAyBI1B,mBAAA,CAES;IAFAmB,OAAK,EAAAQ,MA