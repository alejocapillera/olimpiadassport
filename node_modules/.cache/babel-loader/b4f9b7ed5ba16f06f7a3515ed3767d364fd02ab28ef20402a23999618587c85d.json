{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      pedidos: []\n    };\n  },\n  methods: {\n    async fetchPedidos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pedidos\");\n        this.pedidos = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener los pedidos:\", error);\n      }\n    },\n    verEstadoPedido(idPedido) {\n      // Lógica para ver el estado del pedido\n      console.log(\"Ver estado del pedido:\", idPedido);\n    },\n    realizarEntrega(idPedido) {\n      // Lógica para realizar la entrega del pedido\n      console.log(\"Realizar entrega del pedido:\", idPedido);\n    },\n    verEstadoCuenta(idCliente) {\n      // Lógica para ver el estado de cuenta del cliente\n      console.log(\"Ver estado de cuenta del cliente:\", idCliente);\n    },\n    anularPedido(idPedido) {\n      // Lógica para anular el pedido\n      console.log(\"Anular pedido:\", idPedido);\n    }\n  },\n  mounted() {\n    this.fetchPedidos();\n  }\n};","map":{"version":3,"names":["axios","data","pedidos","methods","fetchPedidos","response","get","error","console","verEstadoPedido","idPedido","log","realizarEntrega","verEstadoCuenta","idCliente","anularPedido","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ListaVentas.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h1>Lista de Ventas</h1>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>ID Pedido</th>\r\n            <th>Cliente</th>\r\n            <th>Fecha</th>\r\n            <th>Monto Total</th>\r\n            <th>Estado</th>\r\n            <th>Acciones</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"pedido in pedidos\" :key=\"pedido.id_pedido\">\r\n            <td>{{ pedido.id_pedido }}</td>\r\n            <td>{{ pedido.nombre_cliente }}</td>\r\n            <td>{{ pedido.fecha_venta }}</td>\r\n            <td>{{ pedido.monto_total }}</td>\r\n            <td>{{ pedido.estado }}</td>\r\n            <td>\r\n              <button @click=\"verEstadoPedido(pedido.id_pedido)\">Ver Estado</button>\r\n              <button @click=\"realizarEntrega(pedido.id_pedido)\" :disabled=\"pedido.estado !== 'Pendiente'\">Realizar Entrega</button>\r\n              <button @click=\"verEstadoCuenta(pedido.id_cliente)\">Ver Estado de Cuenta</button>\r\n              <button @click=\"anularPedido(pedido.id_pedido)\">Anular Pedido</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        pedidos: [],\r\n      };\r\n    },\r\n    methods: {\r\n      async fetchPedidos() {\r\n        try {\r\n          const response = await axios.get(\"http://localhost:3001/api/pedidos\");\r\n          this.pedidos = response.data;\r\n        } catch (error) {\r\n          console.error(\"Error al obtener los pedidos:\", error);\r\n        }\r\n      },\r\n      verEstadoPedido(idPedido) {\r\n        // Lógica para ver el estado del pedido\r\n        console.log(\"Ver estado del pedido:\", idPedido);\r\n      },\r\n      realizarEntrega(idPedido) {\r\n        // Lógica para realizar la entrega del pedido\r\n        console.log(\"Realizar entrega del pedido:\", idPedido);\r\n      },\r\n      verEstadoCuenta(idCliente) {\r\n        // Lógica para ver el estado de cuenta del cliente\r\n        console.log(\"Ver estado de cuenta del cliente:\", idCliente);\r\n      },\r\n      anularPedido(idPedido) {\r\n        // Lógica para anular el pedido\r\n        console.log(\"Anular pedido:\", idPedido);\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchPedidos();\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 10px;\r\n    border: 1px solid #ddd;\r\n  }\r\n  \r\n  th {\r\n    background-color: #f4f4f4;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAkCE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,GAAG,CAAC,mCAAmC,CAAC;QACrE,IAAI,CAACJ,OAAM,GAAIG,QAAQ,CAACJ,IAAI;MAC9B,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACDE,eAAeA,CAACC,QAAQ,EAAE;MACxB;MACAF,OAAO,CAACG,GAAG,CAAC,wBAAwB,EAAED,QAAQ,CAAC;IACjD,CAAC;IACDE,eAAeA,CAACF,QAAQ,EAAE;MACxB;MACAF,OAAO,CAACG,GAAG,CAAC,8BAA8B,EAAED,QAAQ,CAAC;IACvD,CAAC;IACDG,eAAeA,CAACC,SAAS,EAAE;MACzB;MACAN,OAAO,CAACG,GAAG,CAAC,mCAAmC,EAAEG,SAAS,CAAC;IAC7D,CAAC;IACDC,YAAYA,CAACL,QAAQ,EAAE;MACrB;MACAF,OAAO,CAACG,GAAG,CAAC,gBAAgB,EAAED,QAAQ,CAAC;IACzC;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACZ,YAAY,CAAC,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}