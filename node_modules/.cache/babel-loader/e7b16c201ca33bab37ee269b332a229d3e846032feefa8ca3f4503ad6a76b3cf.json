{"ast":null,"code":"import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport axios from 'axios';\nexport default {\n  components: {\n    MenuPrincipal\n  },\n  data() {\n    return {\n      compras: []\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem('token');\n      const id_cliente = localStorage.getItem('id_cliente');\n      if (!token || !id_cliente) {\n        alert('Por favor, inicie sesión para continuar.');\n        return;\n      }\n      try {\n        const response = await axios.get(`http://localhost:3001/api/auth/compras/${id_cliente}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (Array.isArray(response.data)) {\n          this.compras = response.data;\n        } else {\n          console.error('Respuesta inesperada:', response.data);\n        }\n      } catch (error) {\n        console.error('Error al obtener las compras:', error);\n      }\n    },\n    estadoCompra(id_estado) {\n      const estados = {\n        1: 'Pendiente',\n        2: 'En Camino',\n        3: 'Entregado',\n        4: 'Cancelado'\n      };\n      return estados[id_estado] || 'Desconocido';\n    }\n  },\n  mounted() {\n    this.fetchCompras();\n  }\n};","map":{"version":3,"names":["MenuPrincipal","axios","components","data","compras","methods","fetchCompras","token","localStorage","getItem","id_cliente","alert","response","get","headers","Authorization","Array","isArray","console","error","estadoCompra","id_estado","estados","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\MisCompras.vue"],"sourcesContent":["<template>\r\n    <MenuPrincipal />\r\n    <div class=\"mis-compras\">\r\n      <h2>Mis Compras</h2>\r\n      <div v-if=\"compras.length > 0\">\r\n        <div v-for=\"compra in compras\" :key=\"compra.id_venta\" class=\"compra-card\">\r\n          <h3>Compra #{{ compra.id_venta }}</h3>\r\n          <p>Fecha: {{ compra.fecha_venta }}</p>\r\n          <p>Monto Total: ${{ compra.monto_total }}</p>\r\n          <p>Estado: {{ estadoCompra(compra.id_estado) }}</p>\r\n          <div v-if=\"compra.productos.length > 0\">\r\n            <h4>Productos:</h4>\r\n            <ul>\r\n              <li v-for=\"producto in compra.productos\" :key=\"producto.id_producto\">\r\n                {{ producto.descripcion }} - ${{ producto.precio_unitario }} x {{ producto.cantidad }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-else>\r\n        <p>No tienes compras registradas.</p>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    components: {\r\n    MenuPrincipal,\r\n    },\r\n    data() {\r\n      return {\r\n        compras: [],\r\n      };\r\n    },\r\n    methods: {\r\n      async fetchCompras() {\r\n        const token = localStorage.getItem('token');\r\n        const id_cliente = localStorage.getItem('id_cliente');\r\n  \r\n        if (!token || !id_cliente) {\r\n          alert('Por favor, inicie sesión para continuar.');\r\n          return;\r\n        }\r\n  \r\n        try {\r\n          const response = await axios.get(`http://localhost:3001/api/auth/compras/${id_cliente}`, {\r\n            headers: {\r\n              Authorization: `Bearer ${token}`,\r\n            },\r\n          });\r\n  \r\n          if (Array.isArray(response.data)) {\r\n            this.compras = response.data;\r\n          } else {\r\n            console.error('Respuesta inesperada:', response.data);\r\n          }\r\n        } catch (error) {\r\n          console.error('Error al obtener las compras:', error);\r\n        }\r\n      },\r\n      estadoCompra(id_estado) {\r\n        const estados = {\r\n          1: 'Pendiente',\r\n          2: 'En Camino',\r\n          3: 'Entregado',\r\n          4: 'Cancelado',\r\n        };\r\n        return estados[id_estado] || 'Desconocido';\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchCompras();\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .mis-compras {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .compra-card {\r\n    border: 1px solid #ddd;\r\n    border-radius: 5px;\r\n    padding: 15px;\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .compra-card h3 {\r\n    margin-top: 0;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA2BE,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACZF;EACA,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,MAAMC,UAAS,GAAIF,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC;MAErD,IAAI,CAACF,KAAI,IAAK,CAACG,UAAU,EAAE;QACzBC,KAAK,CAAC,0CAA0C,CAAC;QACjD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,0CAA0CH,UAAU,EAAE,EAAE;UACvFI,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC;QACF,CAAC,CAAC;QAEF,IAAIS,KAAK,CAACC,OAAO,CAACL,QAAQ,CAACT,IAAI,CAAC,EAAE;UAChC,IAAI,CAACC,OAAM,GAAIQ,QAAQ,CAACT,IAAI;QAC9B,OAAO;UACLe,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEP,QAAQ,CAACT,IAAI,CAAC;QACvD;MACF,EAAE,OAAOgB,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACDC,YAAYA,CAACC,SAAS,EAAE;MACtB,MAAMC,OAAM,GAAI;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE;MACL,CAAC;MACD,OAAOA,OAAO,CAACD,SAAS,KAAK,aAAa;IAC5C;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACjB,YAAY,CAAC,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}