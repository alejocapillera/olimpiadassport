   <ul>\n        <li v-for=\"(item, index) in carrito\" :key=\"index\">\n          {{ item.descripcion }} - ${{ item.precio_unitario }} x\n          {{ item.cantidad }}\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n\n      <h3>Selecciona el método de pago</h3>\n      <b-form-select v-model=\"metodo_pago_id\">\n        <b-form-select-option value=\"1\"\n          >Tarjeta de Crédito</b-form-select-option\n        >\n        <b-form-select-option value=\"2\">Mercadopago</b-form-select-option>\n        <b-form-select-option value=\"3\">Rapipago</b-form-select-option>\n      </b-form-select>\n\n      <button @click=\"finalizarCompra\" class=\"finalizar-btn\">\n        Finalizar Compra\n      </button>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      carrito: [],\n      totalCarrito: 0,\n      metodo_pago_id: 1,\n    };\n  },\n  methods: {\n    async finalizarCompra() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/venta\",\n          {\n            id_cliente: id_cliente,\n            productos: this.carrito.map((p) => ({\n              id_producto: p.id_producto,\n              cantidad: p.cantidad,\n              precio_unitario: parseFloat(p.precio_unitario),\n            })),\n            total: parseFloat(this.totalCarrito),\n            metodo_pago_id: this.metodo_pago_id,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Venta registrada exitosamente\") {\n          alert(\"Compra finalizada con éxito.\");\n          localStorage.removeItem(\"carrito\");\n          localStorage.removeItem(\"totalCarrito\");\n          this.$router.push({ name: \"home\" });\n        } else {\n          alert(\"Error al finalizar la compra.\");\n        }\n      } catch (error) {\n        console.error(\"Error al finalizar la compra:\", error);\n        alert(\"Error al finalizar la compra.\");\n      }\n    },\n    async cargarCarrito() {\n  const id_cliente = localStorage.getItem(\"id_cliente\");\n  const token = localStorage.getItem(\"token\");\n\n  try {\n    console.log(`Cargando carrito para el cliente con ID: ${id_cliente}`);\n    const response = await axios.get(\n      `http://localhost:3001/api/auth/carrito/${id_cliente}`,\n      {\n        headers: {\n          Authorization: `Bearer ${token}`,\n        },\n      }\n    );\n    console.log(\"Respuesta del servidor al cargar el carrito:\", response);\n\n    // Verifica la estructura de datos de la respuesta\n    if (response.data && Array.isArray(response.data.carrito)) {\n      this.carrito = response.data.carrito;\n      this.totalCarrito = this.carrito.reduce(\n        (total, item) => total + item.precio_unitario * item.cantidad,\n        0\n      );\n    } else {\n      console.error(\n        \"Estructura de la respuesta inesperada:\",\n        response.data\n      );\n      alert(\"Error al cargar el carrito.\");\n    }\n  } catch (error) {\n    console.error(\"Error al cargar el carrito:\", error);\n    alert(\"Error al cargar el carrito.\");\n  }\n}\n\n  },\n  mounted() {\n    const id_cliente = localStorage.getItem(\"id_cliente\");\n\n    if (id_cliente) {\n      this.cargarCarrito();\n    } else {\n      this.$router.push({ name: \"home\" });\n    }\n  },\n};\n</script>\n\n<style scoped>\n.mi-carrito {\n  max-width: 600px;\n  margin: 0 auto;\n  padding: 20px;\n  font-family: Arial, sans-serif;\n}\n\nh2 {\n  text-align: center;\n  color: #333;\n}\n\n.section-usuario,\n.section-resumen {\n  background-color: #f9f9f9;\n  border-radius: 8px;\n  padding: 20px;\n  margin-bottom: 20px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n\n.input-group {\n  margin-bottom: 15px;\n}\n\n.input-group label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\n.input-group input {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  font-size: 16px;\n}\n\n.button-group {\n  display: flex;\n  justify-content: space-between;\n}\n\n.button-group button {\n  background-color: #007bff;\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.button-group button:hover {\n  background-color: #0056b3;\n}\n\n.finalizar-btn {\n  background-color: #28a745;\n}\n\n.finalizar-btn:hover {\n  background-color: #218838;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  margin-bottom: 10px;\n}\n\nb-form-select {\n  width: 100%;\n  margin-bottom: 20px;\n}\n</style>\n"],"mappings":";;;EACOA,KAAK,EAAC;AAAY;gEACrBC,mBAAA,CAAyB,YAArB,kBAAgB;;EAGXD,KAAK,EAAC;AAAiB;gEAC9BC,mBAAA,CAA6B,YAAzB,sBAAoB;gEASxBA,mBAAA,CAAqC,YAAjC,8BAA4B;;;;uBAdpCC,mBAAA,CA2BM,OA3BNC,UA2BM,GA1BJC,UAAyB,EAEzBC,mBAAA,kEAAqE,EACrEJ,mBAAA,CAsBU,WAtBVK,UAsBU,GArBRC,UAA6B,EAC7BN,mBAAA,CAKK,c,kBAJHC,mBAAA,CAGKM,SAAA,QAXbC,WAAA,CAQoCC,KAAA,CAAAC,OAAO,EAR3C,CAQoBC,IAAI,EAAEC,KAAK;yBAAvBX,mBAAA,CAGK;MAHiCY,GAAG,EAAED;IAAK,GAAAE,gBAAA,CAC3CH,IAAI,CAACI,WAAW,IAAG,MAAI,GAAAD,gBAAA,CAAGH,IAAI,CAACK,eAAe,IAAG,KACpD,GAAAF,gBAAA,CAAGH,IAAI,CAACM,QAAQ;oCAGpBjB,mBAAA,CAAiC,WAA9B,UAAQ,GAAAc,gBAAA,CAAGL,KAAA,CAAAS,YAAY,kBAE1BC,UAAqC,EACrCC,YAAA,CAMgBC,wBAAA;IAtBtBC,UAAA,EAgB8Bb,KAAA,CAAAc,cAAc;IAhB5C,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAgB8BhB,KAAA,CAAAc,cAAc,GAAAE,MAAA;;IAhB5CC,OAAA,EAAAC,QAAA,CAiBQ,MAEC,CAFDP,YAAA,CAECQ,+BAAA;MAFqBC,KAAK,EAAC;IAAG;MAjBvCH,OAAA,EAAAC,QAAA,CAkBW,MAAkB,CAlB7BG,gBAAA,CAkBW,oBAAkB,E;MAlB7BC,CAAA;QAoBQX,YAAA,CAAkEQ,+BAAA;MAA5CC,KAAK,EAAC;IAAG;MApBvCH,OAAA,EAAAC,QAAA,CAoBwC,MAAW,CApBnDG,gBAAA,CAoBwC,aAAW,E;MApBnDC,CAAA;QAqBQX,YAAA,CAA+DQ