{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: 0,\n        stock: 0,\n        id_subcategoria: null\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null\n    };\n  },\n  methods: {\n    async cargarCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las categorías:\", error);\n      }\n    },\n    async cargarSubcategorias() {\n      if (!this.selectedCategoria) return;\n      try {\n        const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las subcategorías:\", error);\n      }\n    },\n    async cargarProducto() {\n      const token = localStorage.getItem(\"token\");\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n      try {\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Producto cargado exitosamente.\");\n        this.resetForm();\n      } catch (error) {\n        console.error(\"Error al cargar el producto:\", error);\n        alert(\"Error al cargar el producto.\");\n      }\n    },\n    resetForm() {\n      this.producto = {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: 0,\n        stock: 0,\n        id_subcategoria: null\n      };\n      this.selectedCategoria = null;\n      this.subcategorias = [];\n    }\n  },\n  async mounted() {\n    await this.cargarCategorias();\n  }\n};","map":{"version":3,"names":["axios","data","producto","codigo_producto","descripcion","precio_unitario","stock","id_subcategoria","categorias","subcategorias","selectedCategoria","methods","cargarCategorias","response","get","error","console","cargarSubcategorias","cargarProducto","token","localStorage","getItem","alert","post","headers","Authorization","resetForm","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\CargaProductos.vue"],"sourcesContent":["<template>\n  <div class=\"carga-productos\">\n    <h2>Carga de Productos</h2>\n    <form @submit.prevent=\"cargarProducto\">\n      <div>\n        <label for=\"codigo_producto\">Código del Producto:</label>\n        <input\n          type=\"text\"\n          id=\"codigo_producto\"\n          v-model=\"producto.codigo_producto\"\n          required\n        />\n      </div>\n      <div>\n        <label for=\"descripcion\">Descripción:</label>\n        <textarea\n          id=\"descripcion\"\n          v-model=\"producto.descripcion\"\n          required\n        ></textarea>\n      </div>\n      <div>\n        <label for=\"precio_unitario\">Precio Unitario:</label>\n        <input\n          type=\"number\"\n          id=\"precio_unitario\"\n          v-model=\"producto.precio_unitario\"\n          required\n        />\n      </div>\n      <div>\n        <label for=\"stock\">Stock:</label>\n        <input type=\"number\" id=\"stock\" v-model=\"producto.stock\" required />\n      </div>\n      <div>\n        <label for=\"categoria\">Categoría:</label>\n        <select\n          v-model=\"selectedCategoria\"\n          @change=\"cargarSubcategorias\"\n          required\n        >\n          <option\n            v-for=\"categoria in categorias\"\n            :key=\"categoria.id_categoria\"\n            :value=\"categoria.id_categoria\"\n          >\n            {{ categoria.nombre_categoria }}\n          </option>\n        </select>\n      </div>\n      <div>\n        <label for=\"subcategoria\">Subcategoría:</label>\n        <select v-model=\"producto.id_subcategoria\" required>\n          <option\n            v-for=\"subcategoria in subcategorias\"\n            :key=\"subcategoria.id_subcategoria\"\n            :value=\"subcategoria.id_subcategoria\"\n          >\n            {{ subcategoria.nombre_subcategoria }}\n          </option>\n        </select>\n      </div>\n      <button type=\"submit\">Cargar Producto</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: 0,\n        stock: 0,\n        id_subcategoria: null,\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null,\n    };\n  },\n  methods: {\n    async cargarCategorias() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/categorias\"\n        );\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las categorías:\", error);\n      }\n    },\n    async cargarSubcategorias() {\n      if (!this.selectedCategoria) return;\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/subcategorias/${this.selectedCategoria}`\n        );\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las subcategorías:\", error);\n      }\n    },\n    async cargarProducto() {\n      const token = localStorage.getItem(\"token\");\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n      try {\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        alert(\"Producto cargado exitosamente.\");\n        this.resetForm();\n      } catch (error) {\n        console.error(\"Error al cargar el producto:\", error);\n        alert(\"Error al cargar el producto.\");\n      }\n    },\n    resetForm() {\n      this.producto = {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: 0,\n        stock: 0,\n        id_subcategoria: null,\n      };\n      this.selectedCategoria = null;\n      this.subcategorias = [];\n    },\n  },\n  async mounted() {\n    await this.cargarCategorias();\n  },\n};\n</script>\n\n<style scoped>\n.carga-productos {\n  max-width: 600px;\n  margin: 0 auto;\n  padding: 20px;\n  background-color: #f5f5f5;\n  border-radius: 10px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n\n.carga-productos h2 {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.carga-productos form {\n  display: flex;\n  flex-direction: column;\n}\n\n.carga-productos label {\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\n.carga-productos input,\n.carga-productos textarea,\n.carga-productos select {\n  margin-bottom: 15px;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\n\n.carga-productos button {\n  padding: 10px;\n  background-color: #007bff;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.carga-productos button:hover {\n  background-color: #0056b3;\n}\n</style>\n"],"mappings":"AAoEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,eAAe,EAAE,EAAE;QACnBC,WAAW,EAAE,EAAE;QACfC,eAAe,EAAE,CAAC;QAClBC,KAAK,EAAE,CAAC;QACRC,eAAe,EAAE;MACnB,CAAC;MACDC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAG,CAC9B,sCACF,CAAC;QACD,IAAI,CAACN,UAAS,GAAIK,QAAQ,CAACZ,IAAI;MACjC,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACD,MAAME,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAAC,IAAI,CAACP,iBAAiB,EAAE;MAC7B,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMb,KAAK,CAACc,GAAG