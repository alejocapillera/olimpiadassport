      key: item.id_carrito_producto,\n      class: \"carrito-item\"\n    }, [_createElementVNode(\"div\", _hoisted_14, _toDisplayString(item.descripcion) + \" - $\" + _toDisplayString(item.precio_unitario) + \" x \" + _toDisplayString(item.cantidad), 1 /* TEXT */), _createElementVNode(\"button\", {\n      class: \"btn-eliminar\",\n      onClick: $event => $options.eliminarDelCarrito(index)\n    }, \" Eliminar \", 8 /* PROPS */, _hoisted_15)]);\n  }), 128 /* KEYED_FRAGMENT */))]), _createElementVNode(\"p\", null, \"Total: $\" + _toDisplayString($data.totalCarrito), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_16, [_hoisted_17, _createVNode(_component_b_form_select, {\n    modelValue: $data.metodo_pago_id,\n    \"onUpdate:modelValue\": _cache[7] || (_cache[7] = $event => $data.metodo_pago_id = $event),\n    class: \"form-select\"\n  }, {\n    default: _withCtx(() => [_createVNode(_component_b_form_select_option, {\n      value: \"1\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\"Tarjeta de Crédito\")]),\n      _: 1 /* STABLE */\n    }), _createVNode(_component_b_form_select_option, {\n      value: \"2\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\"Mercadopago\")]),\n      _: 1 /* STABLE */\n    }), _createVNode(_component_b_form_select_option, {\n      value: \"3\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\"Rapipago\")]),\n      _: 1 /* STABLE */\n    })]),\n    _: 1 /* STABLE */\n  }, 8 /* PROPS */, [\"modelValue\"]), _createElementVNode(\"button\", {\n    class: \"btn-procesar\",\n    onClick: _cache[8] || (_cache[8] = (...args) => $options.procesarVenta && $options.procesarVenta(...args))\n  }, \" Finalizar compra \")])], 2 /* CLASS */)]);\n}","map":{"version":3,"names":["_imports_0","class","_createElementVNode","src","alt","_hoisted_10","_createElementBlock","_hoisted_1","_createBlock","_component_b_button","default","_withCtx","_hoisted_2","_createVNode","_component_h8","_createTextVNode","_","_component_b_collapse","id","_hoisted_3","_hoisted_4","_hoisted_5","_hoisted_6","_hoisted_7","onClick","_cache","$event","_ctx","filtrarCategoria","_hoisted_8","_component_b_alert","modelValue","$data","showAlert","variant","show","dismissible","args","$options","toggleSidebar","_hoisted_9","_normalizeClass","sidebarAbierto","_hoisted_11","_hoisted_12","_hoisted_13","_Fragment","_renderList","carrito","item","index","key","id_carrito_producto","_hoisted_14","_toDisplayString","descripcion","precio_unitario","cantidad","eliminarDelCarrito","_hoisted_15","totalCarrito","_hoisted_16","_hoisted_17","_component_b_form_select","metodo_pago_id","_component_b_form_select_option","value","procesarVenta"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ProductosTienda.vue"],"sourcesContent":["<template>\n  <div class=\"productos-tienda\">\n    <b-button v-b-toggle.sidebar-collapse class=\"menu-toggle\">\n      <i class=\"fa-solid fa-bars\"></i>\n      <h8 class=\"h8\">Categorías</h8>\n    </b-button>\n\n    <b-collapse id=\"sidebar-collapse\" class=\"sidebar\">\n      <b-button class=\"close-btn\" v-b-toggle.sidebar-collapse>\n        <i class=\"fa-solid fa-times\"></i>\n      </b-button>\n\n      <section class=\"categorias\">\n        <h2>Categorías</h2>\n        <h5>Seleccione una categoría</h5>\n        <div class=\"categoria-list\">\n          <button class=\"button\" @click=\"filtrarCategoria('Ropa y Accesorios')\">\n            Ropa y Accesorios\n          </button>\n          <button class=\"button\" @click=\"filtrarCategoria('Ciclismo')\">Ciclismo</button>\n          <button class=\"button\" @click=\"filtrarCategoria('Camping')\">Camping</button>\n          <button class=\"button\" @click=\"filtrarCategoria('Caza')\">Caza</button>\n        </div>\n      </section>\n      <img src=\"@/assets/logo.png\" alt=\"Logo\" class=\"logo\" />\n    </b-collapse>\n\n    <b-alert\n      v-model=\"showAlert\"\n      variant=\"success\"\n      show\n      dismissible\n      class=\"alerta-agregado\"\n    >\n      Producto agregado al carrito!\n    </b-alert>\n\n    <button @click=\"toggleSidebar\" class=\"btn-sidebar\">\n      <i class=\"fas fa-shopping-cart icono-carrito\"></i> Ver carrito\n    </button>\n\n    <div :class=\"['sidebar', { 'sidebar-abierto': sidebarAbierto }]\">\n      <button @click=\"toggleSidebar\" class=\"btn-cerrar\">\n        <i class=\"fa-solid fa-times\"></i>\n      </button>\n      <h2>Carrito de compras</h2>\n      <ul class=\"carrito-lista\">\n        <li\n          v-for=\"(item, index) in carrito\"\n          :key=\"item.id_carrito_producto\"\n          class=\"carrito-item\"\n        >\n          <div class=\"carrito-item-info\">\n            {{ item.descripcion }} - ${{ item.precio_unitario }} x\n            {{ item.cantidad }}\n          </div>\n          <button class=\"btn-eliminar\" @click=\"eliminarDelCarrito(index)\">\n            Eliminar\n          </button>\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n      <div class=\"formulario-venta\">\n        <h4>Selecciona el método de pago</h4>\n        <b-form-select v-model=\"metodo_pago_id\" class=\"form-select\">\n          <b-form-select-option value=\"1\">Tarjeta de Crédito</b-form-select-option>\n          <b-form-select-option value=\"2\">Mercadopago</b-form-select-option>\n          <b-form-select-option value=\"3\">Rapipago</b-form-select-option>\n        </b-form-select>\n        <button class=\"btn-procesar\" @click=\"procesarVenta\">\n          Finalizar compra\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { BAlert, BButton, BCollapse, BFormSelect, BFormSelectOption } from \"bootstrap-vue-3\";\n\nexport default {\n  components: {\n    BAlert,\n    BButton,\n    BCollapse,\n    BFormSelect,\n    BFormSelectOption,\n  },\n  props: {\n    categoriaSeleccionada: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      productos: [],\n      carrito: [],\n      totalCarrito: 0,\n      id_cliente: null,\n      sidebarAbierto: false,\n      metodo_pago_id: 1,\n      showAlert: false, // Estado para mostrar la alerta\n    };\n  },\n  computed: {\n    productosFiltrados() {\n      if (this.categoriaSeleccionada) {\n        return this.productos.filter(\n          (producto) =>\n            producto.nombre_subcategoria === this.categoriaSeleccionada\n        );\n      }\n      return this.productos;\n    },\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarAbierto = !this.sidebarAbierto;\n    },\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pro\", {\n          params: {\n            categoria: this.categoriaSeleccionada,\n          },\n        });\n        if (Array.isArray(response.data)) {\n          this.productos = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n    async agregarCarrito(producto) {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      if (!this.id_cliente) {\n        alert(\"El cliente no está definido.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/carrito\",\n          {\n            id_cliente: this.id_cliente,\n            id_producto: producto.id_producto,\n            cantidad: 1,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto agregado al carrito\") {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n          this.showAlert = true; // Mostrar la alerta\n          setTimeout(() => {\n            this.showAlert = false; // Ocultar la alerta después de 3 segundos\n          }, 3000);\n        } else {\n          alert(\"Error al agregar el producto al carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al agregar al carrito:\", error);\n        alert(\"Error al agregar el producto al carrito.\");\n      }\n    },\n    async fetchCarrito() {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !this.id_cliente) {\n        console.error(\"El cliente o el token no están definidos.\");\n        return;\n      }\n\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/auth/carrito`,\n          {\n            params: {\n              id_cliente: this.id_cliente,\n            },\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (Array.isArray(response.data.carrito)) {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n        } else {\n          console.error(\n            \"Respuesta inesperada al obtener el carrito:\",\n            response.data\n          );\n        }\n      } catch (error) {\n        console.error(\"Error al obtener el carrito:\", error);\n      }\n    },\n    async eliminarDelCarrito(index) {\n      const token = localStorage.getItem(\"token\");\n      const producto = this.carrito[index];\n\n      try {\n        const response = await axios.delete(\n          `http://localhost:3001/api/auth/carrito/${producto.id_carrito_producto}`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto eliminado del carrito\") {\n          this.carrito.splice(index, 1);\n          this.actualizarTotal();\n        } else {\n          alert(\"Error al eliminar el producto del carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al eliminar del carrito:\", error);\n        alert(\"Error al eliminar el producto del carrito.\");\n      }\n    },\n    async procesarVenta() {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !this.id_cliente) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/ventas\",\n          {\n            id_cliente: this.id_cliente,\n            carrito: this.carrito,\n            metodo_pago_id: this.metodo_pago_id,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Venta procesada\") {\n          this.carrito = [];\n          this.totalCarrito = 0;\n          alert(\"Compra realizada con éxito.\");\n        } else {\n          alert(\"Error al procesar la venta.\");\n        }\n      } catch (error) {\n        console.error(\"Error al procesar la venta:\", error);\n        alert(\"Error al procesar la venta.\");\n      }\n    },\n    actualizarTotal() {\n      this.totalCarrito = this.carrito.reduce(\n        (total, item) => total + item.precio_unitario * item.cantidad,\n        0\n      );\n    },\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCarrito();\n  },\n};\n</script>\n\n<style scoped>\n/* Agrega aquí tus estilos personalizados */\n</style>\n\n\n<style scoped>\n.menu-toggle {\n  background-color: #0a641a;\n  color: #ff9800;\n  padding: 10px 20px;\n  margin-top: 10px;\n  margin-bottom: 20px;\n  font-size: 16px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease, transform 0.2s ease;\n}\n\n.sidebar {\n  position: fixed;\n  left: 0;\n  top: 0;\n  height: 100%;\n  width: 300px;\n  background-color: #0a641a;\n  box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  z-index: 1001;\n  transition: transform 0.3s;\n  transform: translateX(-100%);\n}\n\n.sidebar.show {\n  transform: translateX(0);\n}\n\n.productos-tienda {\n  padding: 20px;\n  background-color: #ffffff;\n  position: relative;\n}\n\n.productos-grid {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n  justify-content: center;\n}\n\n.producto-card {\n  border: 1px solid #ddd;\n  padding: 20px;\n  width: 250px;\n  text-align: center;\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.2s;\n}\n\n.producto-card:hover {\n  transform: scale(1.05);\n}\n\n.producto-descripcion h3 {\n  font-weight: bold;\n  margin: 0;\n  font-size: 20px;\n  color: #333;\n}\n\n.producto-descripcion p {\n  font-size: 18px;\n  color: #555;\n}\n\n.producto-acciones {\n  margin-top: 15px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.producto-acciones button {\n  padding: 10px 20px;\n  background-color: #0a641a;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  font-weight: bold;\n  transition: background-color 0.3s;\n}\n\n.producto-acciones button:hover {\n  background-color: #09831f;\n}\n\n.icono-carrito {\n  margin-right: 5px;\n  color: #f1c40f;\n}\n\n.btn-sidebar {\n  position: fixed;\n  right: 20px;\n  bottom: 20px;\n  background-color: #0a641a;\n  color: white;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n  border: none;\n  font-size: 16px;\n}\n\n.sidebar {\n  position: fixed;\n  right: -350px;\n  top: 0;\n  height: 100%;\n  width: 350px;\n  background-color: #0a641a;\n  box-shadow: -4px 0 6px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  transition: right 0.3s;\n  z-index: 1000;\n  overflow-y: auto; /* Asegura que el scroll se muestre cuando sea necesario */\n}\n\n/* Estilo para el scroll en la barra lateral */\n.sidebar::-webkit-scrollbar {\n  width: 8px;\n}\n\n.sidebar::-webkit-scrollbar-track {\n  background: #09831f;\n}\n\n.sidebar::-webkit-scrollbar-thumb {\n  background: #f1c40f;\n  border-radius: 4px;\n}\n\n.sidebar::-webkit-scrollbar-thumb:hover {\n  background: #ff9800;\n}\n\n.sidebar-abierto {\n  right: 0;\n}\n\nh2 {\n  color: #fff;\n}\n\np {\n  color: #fff;\n}\n\n.btn-cerrar {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  background-color: transparent;\n  border: none;\n  font-size: 24px;\n  color: #ff9800;\n}\n\n.formulario-venta {\n  color: #fff;\n  margin-top: 20px;\n}\n\n.formulario-venta select {\n  width: 100%;\n  padding: 10px;\n  margin-bottom: 10px;\n}\n\n.btn-procesar {\n  width: 100%;\n  padding: 10px;\n  background-color: #ff9800;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  font-weight: bold;\n  transition: background-color 0.3s;\n}\n\n.btn-procesar:hover {\n  background-color: #f1c40f;\n}\n\n.carrito-lista {\n  list-style-type: none;\n  padding: 0;\n  margin: 0;\n}\n\n.carrito-item {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: #fff;\n  padding: 10px;\n  border-bottom: 1px solid #ddd;\n  border-radius: 5px;\n  margin-bottom: 10px;\n}\n\n.carrito-item-info {\n  flex: 1;\n  color: #333;\n  font-size: 16px;\n}\n\n.btn-eliminar {\n  background-color: #e74c3c;\n  color: white;\n  padding: 5px 10px;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n  font-weight: bold;\n  transition: background-color 0.3s;\n}\n\n.btn-eliminar:hover {\n  background-color: #c0392b;\n}\n\n.alerta-agregado {\n  background-color: #f1c40f;\n  position: fixed;\n  border: none;\n  top: 30px;\n  right: 20px; /* Ajuste para alineación en pantallas más pequeñas */\n  z-index: 1050;\n  width: 300px;\n  padding: 10px; /* Añade un poco de espacio interno */\