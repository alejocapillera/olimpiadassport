{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  methods: {\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/api/productos\");\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","methods","fetchProductos","response","get","productos","data","error","console"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ProductosTienda.vue"],"sourcesContent":["<template>\n  <div class=\"productos-tienda\">\n    <div v-if=\"productos.length > 0\" class=\"productos-grid\">\n      <div\n        v-for=\"producto in productos\"\n        :key=\"producto.id_producto\"\n        class=\"producto-card\"\n      >\n        <div class=\"producto-descripcion\">\n          <h3>{{ producto.nombre }}</h3>\n          <h3>{{ producto.descripcion }}</h3>\n          <p>Precio: ${{ producto.precio_unitario }}</p>\n          <p>Stock: {{ producto.stock }}</p>\n          <p>Categor√≠a: {{ producto.sub_categoria }}</p>\n        </div>\n        <div class=\"producto-acciones\">\n          <button @click=\"agregarCarrito(producto)\">Agregar al carrito</button>\n          <button @click=\"comprarAhora(producto)\">Comprar ahora</button>\n        </div>\n      </div>\n    </div>\n    <div v-else>\n      <p>No hay productos disponibles.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  methods: {\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/api/productos\");\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n  },\n};\n\n</script>\n\n<style scoped>\n.productos-tienda {\n  padding: 20px;\n}\n\n.productos-grid {\n  display: flex;\n  flex-wrap: wrap;\