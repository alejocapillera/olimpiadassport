{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"ListaClientes\",\n  data() {\n    return {\n      clientes: [],\n      error: null\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get(\"http://localhost:3306/api/clientes\");\n      this.clientes = response.data;\n    } catch (error) {\n      this.error = \"Error al obtener los clientes\";\n      console.error(error);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","clientes","error","created","response","get","console"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ListaClientes.vue"],"sourcesContent":["<template>\n  <div class=\"clientes-container\">\n    <h1 class=\"clientes-title\">Lista de Clientes</h1>\n    <div v-if=\"clientes.length\" class=\"table-wrapper\">\n      <table class=\"clientes-table\">\n        <thead>\n          <tr>\n            <th>ID</th>\n            <th>Nombre</th>\n            <th>Email</th>\n            <th>Fecha de Registro</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"cliente in clientes\" :key=\"cliente.id_cliente\">\n            <td>{{ cliente.id_cliente }}</td>\n            <td>{{ cliente.nombre }}</td>\n            <td>{{ cliente.email }}</td>\n            <td>{{ cliente.fecha_registro }}</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n    <p v-else class=\"no-clients-message\">No se encontraron clientes.</p>\n    <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"ListaClientes\",\n  data() {\n    return {\n      clientes: [],\n      error: null,\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get(\"http://localhost:3306/api/clientes\");\n      this.clientes = response.data;\n    } catch (error) {\n      this.error = \"Error al obtener los clientes\";\n      console.error(error);\n    }\n  },\n};\n</script>\n\n<style scoped>\n.clientes-container {\n  padding: 20px;\n  background-color: #f4f4f4;\n}\n\n.clientes-title {\n  text-align: center;\n  color: #333;\n  margin-bottom: 20px;\n}\n\n.table-wrapper {\n  margin: 0 auto;\n  max-width: 100%;\n  overflow-x: auto;\n}\n\n.clientes-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.clientes-table th,\n.clientes-table td {\n  padding: 12px;\n  border: 1px solid #ddd;\n}\n\n.clientes-table th {\n  background-color: #0a641a;\n  color: #fff;\n}\n\n.no-clients-message {\n  text-align: center;\n  color: #333;\n}\n\n.error-message {\n  text-align: center;\n  color: #e74c3c;\n}\n</style>\n"],"mappings":"AA6BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,oCAAoC,CAAC;MACtE,IAAI,CAACJ,QAAO,GAAIG,QAAQ,CAACJ,IAAI;IAC/B,EAAE,OAAOE,KAAK,EAAE;MACd,IAAI,CAACA,KAAI,GAAI,+BAA+B;MAC5CI,OAAO,CAACJ,KAAK,CAACA,KAAK,CAAC;IACtB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}