{"ast":null,"code":"import axios from \"axios\";\nimport MenuPrincipal from \"./MenuPrincipal.vue\";\nimport CustomScroll from \"./CustomScroll.vue\";\nexport default {\n  components: {\n    MenuPrincipal,\n    CustomScroll\n  },\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: null,\n        stock: null,\n        id_subcategoria: null\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null\n    };\n  },\n  methods: {\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error);\n      }\n    },\n    async fetchSubcategorias() {\n      try {\n        const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener subcategorías:\", error);\n      }\n    },\n    async submitForm() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Producto cargado exitosamente\");\n        this.producto = {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: null,\n          stock: null,\n          id_subcategoria: null\n        };\n        this.selectedCategoria = null;\n        this.subcategorias = [];\n      } catch (error) {\n        console.error(\"Error al cargar producto:\", error);\n        alert(\"Hubo un error al cargar el producto\");\n      }\n    }\n  },\n  mounted() {\n    this.fetchCategorias();\n  }\n};","map":{"version":3,"names":["axios","MenuPrincipal","CustomScroll","components","data","producto","codigo_producto","descripcion","precio_unitario","stock","id_subcategoria","categorias","subcategorias","selectedCategoria","methods","fetchCategorias","response","get","error","console","fetchSubcategorias","submitForm","token","localStorage","getItem","post","headers","Authorization","alert","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\CargaProductos.vue"],"sourcesContent":["<template>\n  <custom-scroll>\n    <MenuPrincipal />\n    <div class=\"form-container\">\n      <h1 class=\"form-title\">Cargar Producto</h1>\n      <form @submit.prevent=\"submitForm\" class=\"product-form\">\n        <div class=\"form-group\">\n          <label for=\"codigo_producto\">Codigo de Producto:</label>\n          <input\n            type=\"text\"\n            v-model=\"producto.codigo_producto\"\n            class=\"form-input\"\n            required\n          />\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"descripcion\">Descripcion:</label>\n          <textarea\n            v-model=\"producto.descripcion\"\n            class=\"form-input\"\n            required\n          ></textarea>\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"precio_unitario\">Precio Unitario:</label>\n          <input\n            type=\"number\"\n            v-model=\"producto.precio_unitario\"\n            class=\"form-input\"\n            step=\"0.01\"\n            required\n          />\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"stock\">Stock:</label>\n          <input\n            type=\"number\"\n            v-model=\"producto.stock\"\n            class=\"form-input\"\n            required\n          />\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"id_categoria\">Categoria:</label>\n          <select\n            v-model=\"selectedCategoria\"\n            @change=\"fetchSubcategorias\"\n            class=\"form-select\"\n          >\n            <option\n              v-for=\"categoria in categorias\"\n              :key=\"categoria.id_categoria\"\n              :value=\"categoria.id_categoria\"\n            >\n              {{ categoria.nombre_categoria }}\n            </option>\n          </select>\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"id_subcategoria\">Subcategoria:</label>\n          <select v-model=\"producto.id_subcategoria\" class=\"form-select\">\n            <option\n              v-for=\"subcategoria in subcategorias\"\n              :key=\"subcategoria.id_subcategoria\"\n              :value=\"subcategoria.id_subcategoria\"\n            >\n              {{ subcategoria.nombre_subcategoria }}\n            </option>\n          </select>\n        </div>\n\n        <button type=\"submit\" class=\"submit-button\">Cargar Producto</button>\n      </form>\n    </div>\n  </custom-scroll>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport MenuPrincipal from \"./MenuPrincipal.vue\";\nimport CustomScroll from \"./CustomScroll.vue\";\n\nexport default {\n  components: {\n    MenuPrincipal,\n    CustomScroll,\n  },\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: null,\n        stock: null,\n        id_subcategoria: null,\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null,\n    };\n  },\n  methods: {\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/categorias\"\n        );\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error);\n      }\n    },\n    async fetchSubcategorias() {\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/subcategorias/${this.selectedCategoria}`\n        );\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener subcategorías:\", error);\n      }\n    },\n    async submitForm() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        alert(\"Producto cargado exitosamente\");\n\n        this.producto = {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: null,\n          stock: null,\n          id_subcategoria: null,\n        };\n        this.selectedCategoria = null;\n        this.subcategorias = [];\n      } catch (error) {\n        console.error(\"Error al cargar producto:\", error);\n        alert(\"Hubo un error al cargar el producto\");\n      }\n    },\n  },\n  mounted() {\n    this.fetchCategorias();\n  },\n};\n</script>\n\n<style scoped>\n.form-container {\n  max-width: 600px;\n  margin: 50px auto;\n  padding: 20px;\n  background-color: #f9f9f9;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.form-title {\n  text-align: center;\n  margin-bottom: 20px;\n  font-size: 1.8rem;\n  color: #333;\n}\n\n.product-form {\n  display: flex;\n  flex-direction: column;\n}\n\n.form-group {\n  margin-bottom: 15px;\n}\n\n.form-group label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n  color: #555;\n}\n\n.form-input,\n.form-select {\n  width: 100%;\n  padding: 10px;\n  font-size: 1rem;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  box-sizing: border-box;\n  transition: border-color 0.3s;\n}\n\n.form-input:focus,\n.form-select:focus {\n  border-color: #007bff;\n  outline: none;\n}\n\ntextarea.form-input {\n  resize: vertical;\n}\n\n.submit-button {\n  padding: 10px 15px;\n  font-size: 1rem;\n  color: #fff;\n  background-color: #007bff;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s;\n}\n\n.submit-button:hover {\n  background-color: #0056b3;\n}\n\n.submit-button:active {\n  background-color: #004494;\n}\n</style>\n"],"mappings":"AAmFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,YAAW,MAAO,oBAAoB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVF,aAAa;IACbC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,eAAe,EAAE,EAAE;QACnBC,WAAW,EAAE,EAAE;QACfC,eAAe,EAAE,IAAI;QACrBC,KAAK,EAAE,IAAI;QACXC,eAAe,EAAE;MACnB,CAAC;MACDC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAC9B,sCACF,CAAC;QACD,IAAI,CAACN,UAAS,GAAIK,QAAQ,CAACZ,IAAI;MACjC,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;MACtD;IACF,CAAC;IACD,MAAME,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAMJ,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAC9B,2CAA2C,IAAI,CAACJ,iBAAiB,EACnE,CAAC;QACD,IAAI,CAACD,aAAY,GAAII,QAAQ,CAACZ,IAAI;MACpC,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACD,MAAMG,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMxB,KAAK,CAACyB,IAAI,CAAC,qCAAqC,EAAE,IAAI,CAACpB,QAAQ,EAAE;UACrEqB,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUL,KAAK;UAChC;QACF,CAAC,CAAC;QACFM,KAAK,CAAC,+BAA+B,CAAC;QAEtC,IAAI,CAACvB,QAAO,GAAI;UACdC,eAAe,EAAE,EAAE;UACnBC,WAAW,EAAE,EAAE;UACfC,eAAe,EAAE,IAAI;UACrBC,KAAK,EAAE,IAAI;UACXC,eAAe,EAAE;QACnB,CAAC;QACD,IAAI,CAACG,iBAAgB,GAAI,IAAI;QAC7B,IAAI,CAACD,aAAY,GAAI,EAAE;MACzB,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjDU,KAAK,CAAC,qCAAqC,CAAC;MAC9C;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACd,eAAe,CAAC,CAAC;EACxB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}