eElementVNode(\"p\", null, \"Total: $\" + _toDisplayString($data.totalCarrito), 1 /* TEXT */), _createElementVNode(\"div\", _hoisted_22, [_hoisted_23, _createVNode(_component_b_form_select, {\n    modelValue: $data.metodo_pago_id,\n    \"onUpdate:modelValue\": _cache[3] || (_cache[3] = $event => $data.metodo_pago_id = $event),\n    class: \"form-select\"\n  }, {\n    default: _withCtx(() => [_createVNode(_component_b_form_select_option, {\n      value: \"1\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\" Tarjeta de Crédito \")]),\n      _: 1 /* STABLE */\n    }), _createVNode(_component_b_form_select_option, {\n      value: \"2\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\" Mercadopago \")]),\n      _: 1 /* STABLE */\n    }), _createVNode(_component_b_form_select_option, {\n      value: \"3\"\n    }, {\n      default: _withCtx(() => [_createTextVNode(\" Rapipago \")]),\n      _: 1 /* STABLE */\n    })]),\n    _: 1 /* STABLE */\n  }, 8 /* PROPS */, [\"modelValue\"]), _createElementVNode(\"button\", {\n    class: \"btn-procesar\",\n    onClick: _cache[4] || (_cache[4] = (...args) => $options.procesarVenta && $options.procesarVenta(...args))\n  }, \" Finalizar compra \")])], 2 /* CLASS */)]);\n}","map":{"version":3,"names":["_imports_0","class","_createElementVNode","src","alt","key","_hoisted_13","_hoisted_16","_createElementBlock","_hoisted_1","_hoisted_2","_createBlock","_component_b_button","default","_withCtx","_hoisted_3","_createVNode","_component_h8","_createTextVNode","_","_component_b_collapse","id","_hoisted_4","_hoisted_5","_hoisted_6","_createCommentVNode","_component_b_alert","modelValue","$data","showAlert","_cache","$event","variant","show","dismissible","$options","productosFiltrados","length","_hoisted_7","_Fragment","_renderList","producto","id_producto","_hoisted_8","_toDisplayString","descripcion","precio_unitario","_hoisted_9","onClick","agregarCarrito","_hoisted_11","_hoisted_10","_hoisted_12","_hoisted_14","args","toggleSidebar","_hoisted_15","_normalizeClass","sidebarAbierto","_hoisted_17","_hoisted_18","_hoisted_19","carrito","item","index","id_carrito_producto","_hoisted_20","cantidad","eliminarDelCarrito","_hoisted_21","totalCarrito","_hoisted_22","_hoisted_23","_component_b_form_select","metodo_pago_id","_component_b_form_select_option","value","procesarVenta"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ProductosTienda.vue"],"sourcesContent":["<template>\n  <div class=\"productos-tienda\">\n    <div class=\"sidebar-container\">\n      <b-button v-b-toggle.sidebar-collapse class=\"menu-toggle\">\n        <i class=\"fa-solid fa-bars\"></i>\n        <h8 class=\"h8\">Categorías</h8>\n      </b-button>\n\n      <b-collapse id=\"sidebar-collapse\" class=\"sidebar\">\n        <b-button class=\"close-btn\" v-b-toggle.sidebar-collapse>\n          <i class=\"fa-solid fa-times\"></i>\n        </b-button>\n\n        <section class=\"categorias\">\n          <h2>Categorías</h2>\n          <h5>Seleccione una categoría</h5>\n          <div class=\"categoria-list\">\n            <button class=\"button\" data-text=\"Ropa y Accesorios\">\n              Ropa y Accesorios\n            </button>\n            <button class=\"button\" data-text=\"Ciclismo\">Ciclismo</button>\n            <button class=\"button\" data-text=\"Camping\">Camping</button>\n            <button class=\"button\" data-text=\"Caza\">Caza</button>\n          </div>\n        </section>\n        <img src=\"@/assets/logo.png\" alt=\"Logo\" class=\"logo\" />\n      </b-collapse>\n    </div>\n\n    <!-- Alerta de Bootstrap -->\n    <b-alert\n      v-model=\"showAlert\"\n      variant=\"success\"\n      show\n      dismissible\n      class=\"alerta-agregado\"\n    >\n      Producto agregado al carrito!\n    </b-alert>\n\n    <!-- Contenedor de productos -->\n    <div v-if=\"productosFiltrados.length > 0\" class=\"productos-grid\">\n      <div\n        v-for=\"producto in productosFiltrados\"\n        :key=\"producto.id_producto\"\n        class=\"producto-card\"\n      >\n        <div class=\"producto-descripcion\">\n          <h3>{{ producto.descripcion }}</h3>\n          <p>Precio: ${{ producto.precio_unitario }}</p>\n        </div>\n        <div class=\"producto-acciones\">\n          <button @click=\"agregarCarrito(producto)\" class=\"btn-agregar\">\n            <i class=\"fas fa-shopping-cart icono-carrito\"></i> Agregar al\n            carrito\n          </button>\n        </div>\n      </div>\n    </div>\n    <div v-else>\n      <p>No hay productos disponibles.</p>\n    </div>\n\n    <button @click=\"toggleSidebar\" class=\"btn-sidebar\">\n      <i class=\"fas fa-shopping-cart icono-carrito\"></i> Ver carrito\n    </button>\n\n    <div :class=\"['sidebar', { 'sidebar-abierto': sidebarAbierto }]\">\n      <button @click=\"toggleSidebar\" class=\"btn-cerrar\">\n        <i class=\"fa-solid fa-times\"></i>\n      </button>\n      <h2>Carrito de compras</h2>\n      <ul class=\"carrito-lista\">\n        <li\n          v-for=\"(item, index) in carrito\"\n          :key=\"item.id_carrito_producto\"\n          class=\"carrito-item\"\n        >\n          <div class=\"carrito-item-info\">\n            {{ item.descripcion }} - ${{ item.precio_unitario }} x\n            {{ item.cantidad }}\n          </div>\n          <button class=\"btn-eliminar\" @click=\"eliminarDelCarrito(index)\">\n            Eliminar\n          </button>\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n      <div class=\"formulario-venta\">\n        <h4>Selecciona el método de pago</h4>\n        <b-form-select v-model=\"metodo_pago_id\" class=\"form-select\">\n          <b-form-select-option value=\"1\">\n            Tarjeta de Crédito\n          </b-form-select-option>\n          <b-form-select-option value=\"2\"> Mercadopago </b-form-select-option>\n          <b-form-select-option value=\"3\"> Rapipago </b-form-select-option>\n        </b-form-select>\n        <button class=\"btn-procesar\" @click=\"procesarVenta\">\n          Finalizar compra\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { BAlert, BFormSelect, BFormSelectOption } from \"bootstrap-vue-3\";\n\nexport default {\n  components: {\n    BAlert,\n    BFormSelect,\n    BFormSelectOption,\n  },\n  props: {\n    categoriaSeleccionada: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      productos: [],\n      carrito: [],\n      totalCarrito: 0,\n      id_cliente: null,\n      sidebarAbierto: false,\n      metodo_pago_id: 1,\n      showAlert: false, // Estado para mostrar la alerta\n    };\n  },\n  computed: {\n    productosFiltrados() {\n      if (this.categoriaSeleccionada) {\n        return this.productos.filter(\n          (producto) =>\n            producto.nombre_subcategoria === this.categoriaSeleccionada\n        );\n      }\n      return this.productos;\n    },\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarAbierto = !this.sidebarAbierto;\n    },\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pro\", {\n          params: {\n            categoria: this.categoriaSeleccionada,\n          },\n        });\n        if (Array.isArray(response.data)) {\n          this.productos = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n    async agregarCarrito(producto) {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      if (!this.id_cliente) {\n        alert(\"El cliente no está definido.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/carrito\",\n          {\n            id_cliente: this.id_cliente,\n            id_producto: producto.id_producto,\n            cantidad: 1,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto agregado al carrito\") {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n          this.showAlert = true; // Mostrar la alerta\n          setTimeout(() => {\n            this.showAlert = false; // Ocultar la alerta después de 3 segundos\n          }, 3000);\n        } else {\n          alert(\"Error al agregar el producto al carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al agregar al carrito:\", error);\n        alert(\"Error al agregar el producto al carrito.\");\n      }\n    },\n    async fetchCarrito() {\n      const token = localStorage.getItem(\"token\");\n      this.id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !this.id_cliente) {\n        console.error(\"El cliente o el token no están definidos.\");\n        return;\n      }\n\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/auth/carrito`,\n          {\n            params: {\n              id_cliente: this.id_cliente,\n            },\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (Array.isArray(response.data.carrito)) {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n        } else {\n          console.error(\n            \"Respuesta inesperada al obtener el carrito:\",\n            response.data\n          );\n        }\n      } catch (error) {\n        console.error(\"Error al obtener el carrito:\", error);\n      }\n    },\n    async eliminarDelCarrito(index) {\n      const token = localStorage.getItem(\"token\");\n      const producto = this.carrito[index];\n\n      try {\n        const response = await axios.delete(\n          `http://localhost:3001/api/auth/carrito/${producto.id_carrito_producto}`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Producto eliminado del carrito\") {\n          this.carrito.splice(index, 1);\n          this.actualizarTotal();\n        } else {\n          alert(\"Error al eliminar el producto del carrito.\");\n        }\n      } catch (error) {\n        console.error(\"Error al eliminar del carrito:\", error);\n        alert(\"Error al eliminar el producto del carrito.\");\n      }\n    },\n    actualizarTotal() {\n      this.totalCarrito = this.carrito.reduce((total, item) => {\n        return total + item.precio_unitario * item.cantidad;\n      }, 0);\n    },\n    async procesarVenta() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !id_cliente) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/venta\",\n          {\n            id_cliente,\n            productos: this.carrito,\n            total: this.totalCarrito,\n            metodo_pago_id: this.metodo_pago_id,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Venta registrada exitosamente\") {\n          alert(\"Compra realizada exitosamente!\");\n          this.carrito = [];\n          this.totalCarrito = 0;\n          this.metodo_pago_id = 1;\n        } else {\n          alert(\"Error al procesar la compra.\");\n        }\n      } catch (error) {\n        console.error(\"Error al procesar la compra:\", error);\n        alert(\"Error al procesar la compra.\");\n      }\n    },\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCarrito();\n  },\n};\n</script>\n\n<style scoped>\n.menu-toggle {\n  background-color: #0a641a;\n  color: #ff9800;\n  position: fixed;\n  border: transparent;\n  left: 15px;\n  top: 20px;\n  z-index: 100;\n  padding: 10px;\n  display: bloc;\n  align-items: center;\n  cursor: pointer;\n}\n\n.sidebar-container {\n  position: relative;\n}\n\n.sidebar {\n  position: fixed;\n  left: 0;\n  top: 0;\n  height: 100%;\n  width: 300px;\n  background-color: #0a641a;\n  box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1);\n  padding: 20px;\n  z-index: 1001;\n  transition: transform 0.3s;\n  transform: translateX(-100%);\n}\n\n.sidebar.show {\n  transform: translateX(0);\n}\n  \n.productos-tienda {\n  padding: 20px;\n  background-color: #ffffff;\n  position: relative;\n}\n\n.productos-grid {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 20px;\n  justify-content: center;\n}\n\n.producto-card {\n  border: 1px solid #ddd;\n  padding: 20px;\n  width: 250px;\n  text-align: center;\n  background-color: #fff;\n  border-radius: 10px;\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\n  transition: transform 0.2s;\n}\n\n.producto-card:hover {\n  transform: scale(1.05);\n}\n\n.producto-descripcion h3 {\n  font-weight: bold;\n  margin: 0;\n  font-size: 20px;\n  color: #333;\n}\n\n.producto-descripcion p {\n  font-size: 18px;\n  color: #555;\n}\n\n.producto-acciones {\n  margin-top: 15px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.producto-acciones button {\n  padding: 10px 20px;\n  background-color: #0a641a;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  font-weight: bold;\n  transition: background-color 0.3s;\n}\n\n.producto-acciones button:hover {\n  background-color: #09831f;\n}\n\n.icono-carrito {\n  margin-right: 5