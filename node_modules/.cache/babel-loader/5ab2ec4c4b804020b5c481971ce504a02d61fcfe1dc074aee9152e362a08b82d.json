{"ast":null,"code":"import axios from \"axios\";\nimport MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nexport default {\n  components: {\n    MenuPrincipal\n  },\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: 0,\n        stock: 0,\n        id_subcategoria: null\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null\n    };\n  },\n  methods: {\n    async cargarCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las categorías:\", error);\n      }\n    },\n    async cargarSubcategorias() {\n      if (!this.selectedCategoria) {\n        this.subcategorias = [];\n        return;\n      }\n      try {\n        const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al cargar las subcategorías:\", error);\n      }\n    },\n    async cargarProducto() {\n      const token = localStorage.getItem(\"token\");\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n      try {\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Producto cargado exitosamente.\");\n        // Limpiar el formulario\n        this.producto = {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: 0,\n          stock: 0,\n          id_subcategoria: null\n        };\n        this.selectedCategoria = null;\n        this.subcategorias = [];\n      } catch (error) {\n        console.error(\"Error al cargar el producto:\", error);\n        alert(\"Error al cargar el producto.\");\n      }\n    }\n  },\n  async mounted() {\n    await this.cargarCategorias();\n  }\n};","map":{"version":3,"names":["axios","MenuPrincipal","components","data","producto","codigo_producto","descripcion","precio_unitario","stock","id_subcategoria","categorias","subcategorias","selectedCategoria","methods","cargarCategorias","response","get","error","console","cargarSubcategorias","cargarProducto","token","localStorage","getItem","alert","post","headers","Authorization","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\CargaProductos.vue"],"sourcesContent":["<template>\n    <div class=\"carga-productos\">\n      <h2>Carga de Productos</h2>\n      <form @submit.prevent=\"cargarProducto\">\n        <div>\n          <label for=\"codigo_producto\">Código del Producto:</label>\n          <input type=\"text\" id=\"codigo_producto\" v-model=\"producto.codigo_producto\" required>\n        </div>\n        <div>\n          <label for=\"descripcion\">Descripción:</label>\n          <textarea id=\"descripcion\" v-model=\"producto.descripcion\" required></textarea>\n        </div>\n        <div>\n          <label for=\"precio_unitario\">Precio Unitario:</label>\n          <input type=\"number\" id=\"precio_unitario\" v-model=\"producto.precio_unitario\" required>\n        </div>\n        <div>\n          <label for=\"stock\">Stock:</label>\n          <input type=\"number\" id=\"stock\" v-model=\"producto.stock\" required>\n        </div>\n        <div>\n          <label for=\"id_categoria\">Categoría:</label>\n          <select id=\"id_categoria\" v-model=\"selectedCategoria\" @change=\"cargarSubcategorias\" required>\n            <option value=\"\">Seleccionar Categoría</option>\n            <option v-for=\"categoria in categorias\" :key=\"categoria.id_categoria\" :value=\"categoria.id_categoria\">\n              {{ categoria.nombre_categoria }}\n            </option>\n          </select>\n        </div>\n        <div>\n          <label for=\"id_subcategoria\">Subcategoría:</label>\n          <select id=\"id_subcategoria\" v-model=\"producto.id_subcategoria\" required>\n            <option value=\"\">Seleccionar Subcategoría</option>\n            <option v-for=\"subcategoria in subcategorias\" :key=\"subcategoria.id_subcategoria\" :value=\"subcategoria.id_subcategoria\">\n              {{ subcategoria.nombre_subcategoria }}\n            </option>\n          </select>\n        </div>\n        <button type=\"submit\">Cargar Producto</button>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import axios from \"axios\";\n  import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\n  \n  export default {\n    components: {\n      MenuPrincipal,\n    },\n    data() {\n      return {\n        producto: {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: 0,\n          stock: 0,\n          id_subcategoria: null,\n        },\n        categorias: [],\n        subcategorias: [],\n        selectedCategoria: null,\n      };\n    },\n    methods: {\n      async cargarCategorias() {\n        try {\n          const response = await axios.get(\"http://localhost:3001/api/categorias\");\n          this.categorias = response.data;\n        } catch (error) {\n          console.error(\"Error al cargar las categorías:\", error);\n        }\n      },\n      async cargarSubcategorias() {\n        if (!this.selectedCategoria) {\n          this.subcategorias = [];\n          return;\n        }\n        try {\n          const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n          this.subcategorias = response.data;\n        } catch (error) {\n          console.error(\"Error al cargar las subcategorías:\", error);\n        }\n      },\n      async cargarProducto() {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          alert(\"Por favor, inicie sesión para continuar.\");\n          return;\n        }\n        try {\n          await axios.post(\n            \"http://localhost:3001/api/productos\",\n            this.producto,\n            {\n              headers: {\n                Authorization: `Bearer ${token}`,\n              },\n            }\n          );\n          alert(\"Producto cargado exitosamente.\");\n          // Limpiar el formulario\n          this.producto = {\n            codigo_producto: \"\",\n            descripcion: \"\",\n            precio_unitario: 0,\n            stock: 0,\n            id_subcategoria: null,\n          };\n          this.selectedCategoria = null;\n          this.subcategorias = [];\n        } catch (error) {\n          console.error(\"Error al cargar el producto:\", error);\n          alert(\"Error al cargar el producto.\");\n        }\n      },\n    },\n    async mounted() {\n      await this.cargarCategorias();\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .carga-productos {\n    max-width: 600px;\n    margin: 0 auto;\n    padding: 20px;\n    background-color: #f5f5f5;\n    border-radius: 10px;\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n  }\n  \n  .carga-productos h2 {\n    text-align: center;\n    margin-bottom: 20px;\n  }\n  \n  .carga-productos form {\n    display: flex;\n    flex-direction: column;\n  }\n  \n  .carga-productos label {\n    margin-bottom: 5px;\n    font-weight: bold;\n  }\n  \n  .carga-productos input,\n  .carga-productos textarea,\n  .carga-productos select {\n    margin-bottom: 15px;\n    padding: 10px;\n    border: 1px solid #ccc;\n    border-radius: 5px;\n  }\n  \n  .carga-productos button {\n    padding: 10px;\n    background-color: #007bff;\n    color: white;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n  }\n  \n  .carga-productos button:hover {\n    background-color: #0056b3;\n  }\n  </style>\n  "],"mappings":"AA4CE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CA