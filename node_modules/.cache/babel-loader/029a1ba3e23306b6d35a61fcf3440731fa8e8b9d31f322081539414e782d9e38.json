{"ast":null,"code":"import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport CustomScroll from \"./CustomScroll.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    MenuPrincipal,\n    CustomScroll\n  },\n  data() {\n    return {\n      ventas: []\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/auth/compras\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params: {\n            id_cliente\n          }\n        });\n        this.ventas = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error.response ? error.response.data : error.message);\n        alert(\"Error al obtener las compras.\");\n      }\n    },\n    async cancelarCompra(id_pedido) {\n      const confirmacion = confirm(\"¿Estás seguro de que deseas cancelar esta compra?\");\n      if (!confirmacion) return;\n      const token = localStorage.getItem(\"token\");\n      if (!token) {\n        alert(\"No se puede cancelar la compra. Datos de sesión faltantes.\");\n        return;\n      }\n      try {\n        await axios.delete(`http://localhost:3001/api/pedidos/${id_pedido}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Compra cancelada exitosamente.\");\n        this.fetchCompras(); // Actualizar la lista de compras después de la cancelación\n      } catch (error) {\n        console.error(\"Error al cancelar la compra:\", error.response ? error.response.data : error.message);\n        alert(\"Error al cancelar la compra.\");\n      }\n    },\n    getClassEstado(id_estado) {\n      switch (id_estado) {\n        case 1:\n          return \"estado-pendiente\";\n        case 2:\n          return \"estado-enviado\";\n        case 3:\n          return \"estado-entregado\";\n        case 4:\n          return \"estado-anulado\";\n        default:\n          return \"\";\n      }\n    }\n  },\n  mounted() {\n    this.fetchCompras();\n  }\n};","map":{"version":3,"names":["MenuPrincipal","CustomScroll","axios","components","data","ventas","methods","fetchCompras","token","localStorage","getItem","id_cliente","alert","response","get","headers","Authorization","params","error","console","message","cancelarCompra","id_pedido","confirmacion","confirm","delete","getClassEstado","id_estado","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\MisCompras.vue"],"sourcesContent":["<template>\n  <custom-scroll>\n    <MenuPrincipal />\n    <div class=\"mis-compras\">\n      <h1>Mis Compras</h1>\n      <div v-if=\"ventas.length > 0\">\n        <ul>\n          <li\n            v-for=\"venta in ventas\"\n            :key=\"venta.id_venta\"\n            :class=\"getClassEstado(venta.id_estado)\"\n          >\n            <p>\n              <span>Compra número: {{ venta.id_venta }}</span>\n            </p>\n            <p>\n              <span>Fecha: {{ venta.fecha_venta }}</span>\n            </p>\n            <p>\n              <span>Monto Total: ${{ venta.monto_total }}</span>\n            </p>\n            <p>\n              <span>Estado: {{ venta.estado_nombre }}</span>\n            </p>\n            <!-- Botón para cancelar pedido, oculto si el estado es 'anulado' -->\n            <button\n              v-if=\"venta.id_estado !== 4\"\n              @click=\"cancelarCompra(venta.id_venta)\"\n              class=\"btn-cancelar\"\n            >\n              Cancelar Pedido\n            </button>\n          </li>\n        </ul>\n      </div>\n      <div v-else>\n        <p>No tienes compras registradas.</p>\n      </div>\n    </div>\n  </custom-scroll>\n</template>\n\n<script>\nimport MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport CustomScroll from \"./CustomScroll.vue\";\nimport axios from \"axios\";\n\nexport default {\n  components: {\n    MenuPrincipal,\n    CustomScroll,\n  },\n  data() {\n    return {\n      ventas: [],\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/auth/compras\",\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n            params: {\n              id_cliente,\n            },\n          }\n        );\n        this.ventas = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error.response ? error.response.data : error.message);\n        alert(\"Error al obtener las compras.\");\n      }\n    },\n    async cancelarCompra(id_pedido) {\n      const confirmacion = confirm(\"¿Estás seguro de que deseas cancelar esta compra?\");\n      if (!confirmacion) return;\n\n      const token = localStorage.getItem(\"token\");\n\n      if (!token) {\n        alert(\"No se puede cancelar la compra. Datos de sesión faltantes.\");\n        return;\n      }\n\n      try {\n        await axios.delete(`http://localhost:3001/api/pedidos/${id_pedido}`, {\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        alert(\"Compra cancelada exitosamente.\");\n        this.fetchCompras(); // Actualizar la lista de compras después de la cancelación\n      } catch (error) {\n        console.error(\"Error al cancelar la compra:\", error.response ? error.response.data : error.message);\n        alert(\"Error al cancelar la compra.\");\n      }\n    },\n    getClassEstado(id_estado) {\n      switch (id_estado) {\n        case 1:\n          return \"estado-pendiente\";\n        case 2:\n          return \"estado-enviado\";\n        case 3:\n          return \"estado-entregado\";\n        case 4:\n          return \"estado-anulado\";\n        default:\n          return \"\";\n      }\n    },\n  },\n  mounted() {\n    this.fetchCompras();\n  },\n};\n</script>\n"],"mappings":"AA2CA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVH,aAAa;IACbC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,MAAMC,UAAS,GAAIF,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC;MAErD,IAAI,CAACF,KAAK,EAAE;QACVI,KAAK,CAAC,0CAA0C,CAAC;QACjD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAC9B,wCAAwC,EACxC;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC,CAAC;UACDS,MAAM,EAAE;YACNN;UACF;QACF,CACF,CAAC;QACD,IAAI,CAACN,MAAK,GAAIQ,QAAQ,CAACT,IAAI;MAC7B,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAACL,QAAO,GAAIK,KAAK,CAACL,QAAQ,CAACT,IAAG,GAAIc,KAAK,CAACE,OAAO,CAAC;QACpGR,KAAK,CAAC,+BAA+B,CAAC;MACxC;IACF,CAAC;IACD,MAAMS,cAAcA,CAACC,SAAS,EAAE;MAC9B,MAAMC,YAAW,GAAIC,OAAO,CAAC,mDAAmD,CAAC;MACjF,IAAI,CAACD,YAAY,EAAE;MAEnB,MAAMf,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAE3C,IAAI,CAACF,KAAK,EAAE;QACVI,KAAK,CAAC,4DAA4D,CAAC;QACnE;MACF;MAEA,IAAI;QACF,MAAMV,KAAK,CAACuB,MAAM,CAAC,qCAAqCH,SAAS,EAAE,EAAE;UACnEP,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC;QACF,CAAC,CAAC;QACFI,KAAK,CAAC,gCAAgC,CAAC;QACvC,IAAI,CAACL,YAAY,CAAC,CAAC,EAAE;MACvB,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAACL,QAAO,GAAIK,KAAK,CAACL,QAAQ,CAACT,IAAG,GAAIc,KAAK,CAACE,OAAO,CAAC;QACnGR,KAAK,CAAC,8BAA8B,CAAC;MACvC;IACF,CAAC;IACDc,cAAcA,CAACC,SAAS,EAAE;MACxB,QAAQA,SAAS;QACf,KAAK,CAAC;UACJ,OAAO,kBAAkB;QAC3B,KAAK,CAAC;UACJ,OAAO,gBAAgB;QACzB,K