{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      productos: [],\n      carrito: [],\n      totalCarrito: 0\n    };\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pro\");\n        if (Array.isArray(response.data)) {\n          this.productos = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n    agregarCarrito(producto) {\n      const itemEnCarrito = this.carrito.find(item => item.id_producto === producto.id_producto);\n      if (itemEnCarrito) {\n        itemEnCarrito.cantidad++;\n      } else {\n        this.carrito.push({\n          ...producto,\n          cantidad: 1\n        });\n      }\n      this.actualizarTotal();\n    },\n    eliminarDelCarrito(index) {\n      this.carrito.splice(index, 1);\n      this.actualizarTotal();\n    },\n    actualizarTotal() {\n      this.totalCarrito = this.carrito.reduce((total, item) => {\n        return total + item.precio_unitario * item.cantidad;\n      }, 0);\n    },\n    async procesarVenta() {\n      if (!localStorage.getItem(\"token\")) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      // Verifica si el usuario está en localStorage\n      const usuario = localStorage.getItem(\"user\");\n      if (!usuario) {\n        alert(\"No se encontró información del usuario.\");\n        return;\n      }\n      let id_cliente;\n      try {\n        const usuarioParsed = JSON.parse(usuario);\n        id_cliente = usuarioParsed.id_cliente;\n      } catch (error) {\n        console.error(\"Error al parsear la información del usuario:\", error);\n        alert(\"Error al procesar la venta.\");\n        return;\n      }\n      try {\n        const metodo_pago_id = 1; // O cualquier valor adecuado según la lógica de tu aplicación\n\n        const response = await axios.post(\"http://localhost:3001/api/venta\", {\n          id_cliente,\n          productos: this.carrito.map(item => ({\n            id_producto: item.id_producto,\n            cantidad: item.cantidad,\n            precio_unitario: item.precio_unitario\n          })),\n          total: this.totalCarrito,\n          metodo_pago_id: metodo_pago_id\n        }, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n          }\n        });\n        if (response.data.message === \"Venta registrada exitosamente\") {\n          alert(\"Venta procesada con éxito.\");\n          this.carrito = [];\n          this.totalCarrito = 0;\n        } else {\n          alert(\"Error al procesar la venta.\");\n        }\n      } catch (error) {\n        console.error(\"Error al procesar la venta:\", error);\n        alert(\"Error al procesar la venta.\");\n      }\n    }\n  },\n  mounted() {\n    this.fetchProductos();\n  }\n};","map":{"version":3,"names":["axios","data","productos","carrito","totalCarrito","methods","fetchProductos","response","get","Array","isArray","console","error","agregarCarrito","producto","itemEnCarrito","find","item","id_producto","cantidad","push","actualizarTotal","eliminarDelCarrito","index","splice","reduce","total","precio_unitario","procesarVenta","localStorage","getItem","alert","usuario","id_cliente","usuarioParsed","JSON","parse","metodo_pago_id","post","map","headers","Authorization","message","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ProductosTienda.vue"],"sourcesContent":["<template>\n  <div class=\"productos-tienda\">\n    <div v-if=\"productos.length > 0\" class=\"productos-grid\">\n      <div\n        v-for=\"producto in productos\"\n        :key=\"producto.id_producto\"\n        class=\"producto-card\"\n      >\n        <div class=\"producto-descripcion\">\n          <h3>{{ producto.descripcion }}</h3>\n          <p>Precio: ${{ producto.precio_unitario }}</p>\n          <p>Stock: {{ producto.stock }}</p>\n        </div>\n        <div class=\"producto-acciones\">\n          <button @click=\"agregarCarrito(producto)\">Agregar al carrito</button>\n          <button @click=\"comprarAhora(producto)\">Comprar ahora</button>\n        </div>\n      </div>\n    </div>\n    <div v-else>\n      <p>No hay productos disponibles.</p>\n    </div>\n\n    <div v-if=\"carrito.length > 0\" class=\"carrito\">\n      <h3>Carrito de compras</h3>\n      <ul>\n        <li v-for=\"(item, index) in carrito\" :key=\"index\">\n          {{ item.descripcion }} - ${{ item.precio_unitario }} x\n          {{ item.cantidad }}\n          <button @click=\"eliminarDelCarrito(index)\">Eliminar</button>\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n      <button @click=\"procesarVenta\">Procesar venta</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      productos: [],\n      carrito: [],\n      totalCarrito: 0,\n    };\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/pro\");\n        if (Array.isArray(response.data)) {\n          this.productos = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error);\n      }\n    },\n    agregarCarrito(producto) {\n      const itemEnCarrito = this.carrito.find(\n        (item) => item.id_producto === producto.id_producto\n      );\n\n      if (itemEnCarrito) {\n        itemEnCarrito.cantidad++;\n      } else {\n        this.carrito.push({ ...producto, cantidad: 1 });\n      }\n\n      this.actualizarTotal();\n    },\n    eliminarDelCarrito(index) {\n      this.carrito.splice(index, 1);\n      this.actualizarTotal();\n    },\n    actualizarTotal() {\n      this.totalCarrito = this.carrito.reduce((total, item) => {\n        return total + item.precio_unitario * item.cantidad;\n      }, 0);\n    },\n    async procesarVenta() {\n  if (!localStorage.getItem(\"token\")) {\n    alert(\"Por favor, inicie sesión para continuar.\");\n    return;\n  }\n\n  // Verifica si el usuario está en localStorage\n  const usuario = localStorage.getItem(\"user\");\n  if (!usuario) {\n    alert(\"No se encontró información del usuario.\");\n    return;\n  }\n\n  let id_cliente;\n  try {\n    const usuarioParsed = JSON.parse(usuario);\n    id_cliente = usuarioParsed.id_cliente;\n  } catch (error) {\n    console.error(\"Error al parsear la información del usuario:\", error);\n    alert(\"Error al procesar la venta.\");\n    return;\n  }\n\n  try {\n    const metodo_pago_id = 1; // O cualquier valor adecuado según la lógica de tu aplicación\n\n    const response = await axios.post(\n      \"http://localhost:3001/api/venta\",\n      {\n        id_cliente,\n        productos: this.carrito.map((item) => ({\n          id_producto: item.id_producto,\n          cantidad: item.cantidad,\n          precio_unitario: item.precio_unitario,\n        })),\n        total: this.totalCarrito,\n        metodo_pago_id: metodo_pago_id,\n      },\n      {\n        headers: {\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n        },\n      }\n    );\n\n    if (response.data.message === \"Venta registrada exitosamente\") {\n      alert(\"Venta procesada con éxito.\");\n      this.carrito = [];\n      this.totalCarrito = 0;\n    } else {\n      alert(\"Error al procesar la venta.\");\n    }\n  } catch (error) {\n    console.error(\"Error al procesar la venta:\", error);\n    alert(\"Error al procesar la venta.\");\n  }\n}\n  },\n  mounted() {\n    this.fetchProductos();\n  },\n};\n</script>\n\n<style scoped>\n.productos-tienda {\n  padding: 20p