{"ast":null,"code":"import axios from \"axios\";\nimport MenuPrincipal from \"./MenuPrincipal.vue\";\nexport default {\n  components: {\n    MenuPrincipal\n  },\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: null,\n        stock: null,\n        id_subcategoria: null\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null\n    };\n  },\n  methods: {\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error);\n      }\n    },\n    async fetchSubcategorias() {\n      try {\n        const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener subcategorías:\", error);\n      }\n    },\n    async submitForm() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Producto cargado exitosamente\");\n\n        // Limpiar los campos del formulario\n        this.producto = {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: null,\n          stock: null,\n          id_subcategoria: null\n        };\n        this.selectedCategoria = null;\n        this.subcategorias = []; // Opcional: Si quieres limpiar las subcategorías seleccionadas\n      } catch (error) {\n        console.error(\"Error al cargar producto:\", error);\n        alert(\"Hubo un error al cargar el producto\");\n      }\n    }\n  },\n  mounted() {\n    this.fetchCategorias();\n  }\n};","map":{"version":3,"names":["axios","MenuPrincipal","components","data","producto","codigo_producto","descripcion","precio_unitario","stock","id_subcategoria","categorias","subcategorias","selectedCategoria","methods","fetchCategorias","response","get","error","console","fetchSubcategorias","submitForm","token","localStorage","getItem","post","headers","Authorization","alert","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\CargaProductos.vue"],"sourcesContent":["<template>\n  <div>\n    <MenuPrincipal />\n    <h1>Cargar Producto</h1>\n    <form @submit.prevent=\"submitForm\">\n      <div>\n        <label for=\"codigo_producto\">Código de Producto:</label>\n        <input type=\"text\" v-model=\"producto.codigo_producto\" required />\n      </div>\n\n      <div>\n        <label for=\"descripcion\">Descripción:</label>\n        <textarea v-model=\"producto.descripcion\" required></textarea>\n      </div>\n\n      <div>\n        <label for=\"precio_unitario\">Precio Unitario:</label>\n        <input\n          type=\"number\"\n          v-model=\"producto.precio_unitario\"\n          step=\"0.01\"\n          required\n        />\n      </div>\n\n      <div>\n        <label for=\"stock\">Stock:</label>\n        <input type=\"number\" v-model=\"producto.stock\" required />\n      </div>\n\n      <div>\n        <label for=\"id_categoria\">Categoría:</label>\n        <select v-model=\"selectedCategoria\" @change=\"fetchSubcategorias\">\n          <option\n            v-for=\"categoria in categorias\"\n            :key=\"categoria.id_categoria\"\n            :value=\"categoria.id_categoria\"\n          >\n            {{ categoria.nombre_categoria }}\n          </option>\n        </select>\n      </div>\n\n      <div>\n        <label for=\"id_subcategoria\">Subcategoría:</label>\n        <select v-model=\"producto.id_subcategoria\">\n          <option\n            v-for=\"subcategoria in subcategorias\"\n            :key=\"subcategoria.id_subcategoria\"\n            :value=\"subcategoria.id_subcategoria\"\n          >\n            {{ subcategoria.nombre_subcategoria }}\n          </option>\n        </select>\n      </div>\n\n      <button type=\"submit\">Cargar Producto</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport MenuPrincipal from \"./MenuPrincipal.vue\";\n\nexport default {\n  components: {\n    MenuPrincipal,\n  },\n  data() {\n    return {\n      producto: {\n        codigo_producto: \"\",\n        descripcion: \"\",\n        precio_unitario: null,\n        stock: null,\n        id_subcategoria: null,\n      },\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null,\n    };\n  },\n  methods: {\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/categorias\"\n        );\n        this.categorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error);\n      }\n    },\n    async fetchSubcategorias() {\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/subcategorias/${this.selectedCategoria}`\n        );\n        this.subcategorias = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener subcategorías:\", error);\n      }\n    },\n    async submitForm() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.post(\"http://localhost:3001/api/productos\", this.producto, {\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        alert(\"Producto cargado exitosamente\");\n\n        // Limpiar los campos del formulario\n        this.producto = {\n          codigo_producto: \"\",\n          descripcion: \"\",\n          precio_unitario: null,\n          stock: null,\n          id_subcategoria: null,\n        };\n        this.selectedCategoria = null;\n        this.subcategorias = []; // Opcional: Si quieres limpiar las subcategorías seleccionadas\n      } catch (error) {\n        console.error(\"Error al cargar producto:\", error);\n        alert(\"Hubo un error al cargar el producto\");\n      }\n    },\n  },\n  mounted() {\n    this.fetchCategorias();\n  },\n};\n</script>\n\n<style scoped>\n/* Agrega tus estilos aquí */\n</style>\n"],"mappings":"AA8DA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,eAAe,EAAE,EAAE;QACnBC,WAAW,EAAE,EAAE;QACfC,eAAe,EAAE,IAAI;QACrBC,KAAK,EAAE,IAAI;QACXC,eAAe,EAAE;MACnB,CAAC;MACDC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE,EAAE;MACjBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAC9B,sCACF,CAAC;QACD,IAAI,CAACN,UAAS,GAAIK,QAAQ,CAACZ,IAAI;MACjC,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;MACtD;IACF,CAAC;IACD,MAAME,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAMJ,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAC9B,2CAA2C,IAAI,CAACJ,iBAAiB,EACnE,CAAC;QACD,IAAI,CAACD,aAAY,GAAII,QAAQ,CAACZ,IAAI;MACpC,EAAE,OAAOc,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACD,MAAMG,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMvB,KAAK,CAACwB,IAAI,CAAC,qCAAqC,EAAE,IAAI,CAACpB,QAAQ,EAAE;UACrEqB,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUL,KAAK;UAChC;QACF,CAAC,CAAC;QACFM,KAAK,CAAC,+BAA+B,CAAC;;QAEtC;QACA,IAAI,CAACvB,QAAO,GAAI;UACdC,eAAe,EAAE,EAAE;UACnBC,WAAW,EAAE,EAAE;UACfC,eAAe,EAAE,IAAI;UACrBC,KAAK,EAAE,IAAI;UACXC,eAAe,EAAE;QACnB,CAAC;QACD,IAAI,CAACG,iBAAgB,GAAI,IAAI;QAC7B,IAAI,CAACD,aAAY,GAAI,EAAE,EAAE;MAC3B,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjDU,KAAK,CAAC,qCAAqC,CAAC;MAC9C;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACd,eAAe,CAAC,CAAC;EACxB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}