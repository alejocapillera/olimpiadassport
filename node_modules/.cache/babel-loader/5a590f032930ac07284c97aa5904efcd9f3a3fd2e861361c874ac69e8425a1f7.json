{"ast":null,"code":"import axios from \"axios\";\nimport { BModal, BButton, BFormGroup, BFormInput, BFormSelect } from \"bootstrap-vue-3\";\nimport Swal from \"sweetalert2\";\nexport default {\n  components: {\n    BModal,\n    BButton,\n    BFormGroup,\n    BFormInput,\n    BFormSelect\n  },\n  data() {\n    return {\n      productos: [],\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null,\n      showModalDetalles: false,\n      showModalEditar: false,\n      productoActual: {},\n      productoAEliminar: {},\n      searchQuery: \"\",\n      // Búsqueda\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {\n    filteredProductos() {\n      return this.productos.filter(producto => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return producto.descripcion.toLowerCase().includes(searchTerm) || producto.codigo_producto.toLowerCase().includes(searchTerm);\n      });\n    },\n    paginatedProductos() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.filteredProductos.slice(start, end);\n    },\n    totalPages() {\n      return Math.ceil(this.filteredProductos.length / this.itemsPerPage);\n    }\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const response = await axios.get(\"http://localhost:3001/api/listaproductos\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error.response ? error.response.data : error.message);\n      }\n    },\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data.map(categoria => ({\n          value: categoria.id_categoria,\n          text: categoria.nombre_categoria\n        }));\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error.response ? error.response.data : error.message);\n      }\n    },\n    async fetchSubcategorias() {\n      if (this.selectedCategoria) {\n        try {\n          const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n          this.subcategorias = response.data.map(subcategoria => ({\n            value: subcategoria.id_subcategoria,\n            text: subcategoria.nombre_subcategoria\n          }));\n        } catch (error) {\n          console.error(\"Error al obtener subcategorías:\", error.response ? error.response.data : error.message);\n        }\n      } else {\n        this.subcategorias = []; // Limpia subcategorías si no hay categoría seleccionada\n      }\n    },\n    verDetallesProducto(producto) {\n      this.productoActual = producto;\n      this.showModalDetalles = true;\n    },\n    abrirModalEditar(producto) {\n      this.productoActual = {\n        ...producto\n      };\n      this.selectedCategoria = producto.id_subcategoria;\n      this.fetchSubcategorias();\n      this.showModalEditar = true;\n    },\n    async editarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.put(`http://localhost:3001/api/productos/${this.productoActual.id_producto}`, this.productoActual, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.fetchProductos();\n        this.showModalEditar = false;\n        Swal.fire({\n          icon: \"success\",\n          title: \"Producto Editado\",\n          text: \"El producto ha sido editado exitosamente.\",\n          confirmButtonColor: \"#0a641a\"\n        });\n      } catch (error) {\n        console.error(\"Error al editar el producto:\", error.response ? error.response.data : error.message);\n        Swal.fire({\n          icon: \"error\",\n          title: \"Error\",\n          text: \"No se pudo editar el producto.\",\n          confirmButtonColor: \"#0a641a\"\n        });\n      }\n    },\n    async confirmarEliminacion(producto) {\n      const result = await Swal.fire({\n        title: \"Confirmar Eliminación\",\n        text: `¿Estás seguro de que deseas eliminar el producto \"${producto.descripcion}\"?`,\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#0a641a\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Eliminar\",\n        cancelButtonText: \"Cancelar\"\n      });\n      if (result.isConfirmed) {\n        this.eliminarProducto(producto);\n      }\n    },\n    async eliminarProducto(producto) {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(`http://localhost:3001/api/productos/${producto.id_producto}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.fetchProductos();\n        Swal.fire({\n          icon: \"success\",\n          title: \"Producto Eliminado\",\n          text: \"El producto ha sido eliminado exitosamente.\",\n          confirmButtonColor: \"#0a641a\"\n        });\n      } catch (error) {\n        console.error(\"Error al eliminar el producto:\", error.response ? error.response.data : error.message);\n        Swal.fire({\n          icon: \"error\",\n          title: \"Error\",\n          text: \"No se pudo eliminar el producto.\",\n          confirmButtonColor: \"#0a641a\"\n        });\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCategorias();\n  }\n};","map":{"version":3,"names":["axios","BModal","BButton","BFormGroup","BFormInput","BFormSelect","Swal","components","data","productos","categorias","subcategorias","selectedCategoria","showModalDetalles","showModalEditar","productoActual","productoAEliminar","searchQuery","currentPage","itemsPerPage","computed","filteredProductos","filter","producto","searchTerm","toLowerCase","descripcion","includes","codigo_producto","paginatedProductos","start","end","slice","totalPages","Math","ceil","length","methods","fetchProductos","token","localStorage","getItem","response","get","headers","Authorization","error","console","message","fetchCategorias","map","categoria","value","id_categoria","text","nombre_categoria","fetchSubcategorias","subcategoria","id_subcategoria","nombre_subcategoria","verDetallesProducto","abrirModalEditar","editarProducto","put","id_producto","fire","icon","title","confirmButtonColor","confirmarEliminacion","result","showCancelButton","cancelButtonColor","confirmButtonText","cancelButtonText","isConfirmed","eliminarProducto","delete","prevPage","nextPage","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ListaVentas.vue"],"sourcesContent":["<template>\n  <div class=\"productos-container\">\n    <h1 class=\"productos-title\">Lista de Productos</h1>\n    \n    <!-- Campo de búsqueda -->\n    <div class=\"search-container\">\n      <input\n        type=\"text\"\n        v-model=\"searchQuery\"\n        placeholder=\"Buscar por descripción o código\"\n        class=\"search-input\"\n      />\n    </div>\n\n    <div class=\"table-container\">\n      <table class=\"productos-table\">\n        <thead>\n          <tr>\n            <th>Descripción</th>\n            <th>Stock</th>\n            <th>Acciones</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"producto in paginatedProductos\" :key=\"producto.id_producto\">\n            <td>{{ producto.descripcion }}</td>\n            <td>{{ producto.stock }}</td>\n            <td>\n              <button @click=\"verDetallesProducto(producto)\" class=\"btn-ver\">\n                Ver Detalles\n              </button>\n              <button @click=\"abrirModalEditar(producto)\" class=\"btn-editar\">\n                Editar\n              </button>\n              <button\n                @click=\"confirmarEliminacion(producto)\"\n                class=\"btn-eliminar\"\n              >\n                Eliminar\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n\n    <!-- Paginación -->\n    <div class=\"pagination-container\">\n      <button @click=\"prevPage\" :disabled=\"currentPage === 1\">Anterior</button>\n      <span>Página {{ currentPage }} de {{ totalPages }}</span>\n      <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">Siguiente</button>\n    </div>\n\n    <!-- Modal para mostrar detalles del producto -->\n    <b-modal\n      id=\"modalDetallesProducto\"\n      title=\"Detalles del Producto\"\n      v-model=\"showModalDetalles\"\n    >\n      <p><strong>Código:</strong> {{ productoActual.codigo_producto }}</p>\n      <p><strong>Descripción:</strong> {{ productoActual.descripcion }}</p>\n      <p>\n        <strong>Precio Unitario:</strong> {{ productoActual.precio_unitario }}\n      </p>\n      <p><strong>Stock:</strong> {{ productoActual.stock }}</p>\n      <p>\n        <strong>Subcategoría:</strong> {{ productoActual.nombre_subcategoria }}\n      </p>\n      <b-button @click=\"showModalDetalles = false\">Cerrar</b-button>\n    </b-modal>\n\n    <!-- Modal para editar producto -->\n    <b-modal\n      id=\"modalEditarProducto\"\n      title=\"Editar Producto\"\n      v-model=\"showModalEditar\"\n      @ok=\"editarProducto\"\n    >\n      <form @submit.prevent=\"editarProducto\">\n        <b-form-group label=\"Código\">\n          <b-form-input\n            v-model=\"productoActual.codigo_producto\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Descripción\">\n          <b-form-input\n            v-model=\"productoActual.descripcion\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Precio Unitario\">\n          <b-form-input\n            type=\"number\"\n            v-model=\"productoActual.precio_unitario\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Stock\">\n          <b-form-input\n            type=\"number\"\n            v-model=\"productoActual.stock\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Categoría\">\n          <b-form-select\n            v-model=\"selectedCategoria\"\n            @change=\"fetchSubcategorias\"\n            :options=\"categorias\"\n            required\n          ></b-form-select>\n        </b-form-group>\n        <b-form-group label=\"Subcategoría\">\n          <b-form-select\n            v-model=\"productoActual.id_subcategoria\"\n            :options=\"subcategorias\"\n            required\n          >\n            <b-form-select-option\n              v-for=\"subcategoria in subcategorias\"\n              :key=\"subcategoria.value\"\n              :value=\"subcategoria.value\"\n            >\n              {{ subcategoria.text }}\n            </b-form-select-option>\n          </b-form-select>\n        </b-form-group>\n      </form>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {\n  BModal,\n  BButton,\n  BFormGroup,\n  BFormInput,\n  BFormSelect,\n} from \"bootstrap-vue-3\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  components: {\n    BModal,\n    BButton,\n    BFormGroup,\n    BFormInput,\n    BFormSelect,\n  },\n  data() {\n    return {\n      productos: [],\n      categorias: [],\n      subcategorias: [],\n      selectedCategoria: null,\n      showModalDetalles: false,\n      showModalEditar: false,\n      productoActual: {},\n      productoAEliminar: {},\n      searchQuery: \"\", // Búsqueda\n      currentPage: 1,\n      itemsPerPage: 10,\n    };\n  },\n  computed: {\n    filteredProductos() {\n      return this.productos.filter((producto) => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return (\n          producto.descripcion.toLowerCase().includes(searchTerm) ||\n          producto.codigo_producto.toLowerCase().includes(searchTerm)\n        );\n      });\n    },\n    paginatedProductos() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.filteredProductos.slice(start, end);\n    },\n    totalPages() {\n      return Math.ceil(this.filteredProductos.length / this.itemsPerPage);\n    },\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const response = await axios.get(\n          \"http://localhost:3001/api/listaproductos\",\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\n          \"Error al obtener los productos:\",\n          error.response ? error.response.data : error.message\n        );\n      }\n    },\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/categorias\"\n        );\n        this.categorias = response.data.map((categoria) => ({\n          value: categoria.id_categoria,\n          text: categoria.nombre_categoria,\n        }));\n      } catch (error) {\n        console.error(\n          \"Error al obtener categorías:\",\n          error.response ? error.response.data : error.message\n        );\n      }\n    },\n    async fetchSubcategorias() {\n      if (this.selectedCategoria) {\n        try {\n          const response = await axios.get(\n            `http://localhost:3001/api/subcategorias/${this.selectedCategoria}`\n          );\n          this.subcategorias = response.data.map((subcategoria) => ({\n            value: subcategoria.id_subcategoria,\n            text: subcategoria.nombre_subcategoria,\n          }));\n        } catch (error) {\n          console.error(\n            \"Error al obtener subcategorías:\",\n            error.response ? error.response.data : error.message\n          );\n        }\n      } else {\n        this.subcategorias = []; // Limpia subcategorías si no hay categoría seleccionada\n      }\n    },\n    verDetallesProducto(producto) {\n      this.productoActual = producto;\n      this.showModalDetalles = true;\n    },\n    abrirModalEditar(producto) {\n      this.productoActual = { ...producto };\n      this.selectedCategoria = producto.id_subcategoria;\n      this.fetchSubcategorias();\n      this.showModalEditar = true;\n    },\n    async editarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.put(\n          `http://localhost:3001/api/productos/${this.productoActual.id_producto}`,\n          this.productoActual,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.fetchProductos();\n        this.showModalEditar = false;\n        Swal.fire({\n          icon: \"success\",\n          title: \"Producto Editado\",\n          text: \"El producto ha sido editado exitosamente.\",\n          confirmButtonColor: \"#0a641a\",\n        });\n      } catch (error) {\n        console.error(\n          \"Error al editar el producto:\",\n          error.response ? error.response.data : error.message\n        );\n        Swal.fire({\n          icon: \"error\",\n          title: \"Error\",\n          text: \"No se pudo editar el producto.\",\n          confirmButtonColor: \"#0a641a\",\n        });\n      }\n    },\n    async confirmarEliminacion(producto) {\n      const result = await Swal.fire({\n        title: \"Confirmar Eliminación\",\n        text: `¿Estás seguro de que deseas eliminar el producto \"${producto.descripcion}\"?`,\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#0a641a\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Eliminar\",\n        cancelButtonText: \"Cancelar\",\n      });\n\n      if (result.isConfirmed) {\n        this.eliminarProducto(producto);\n      }\n    },\n    async eliminarProducto(producto) {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(\n          `http://localhost:3001/api/productos/${producto.id_producto}`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.fetchProductos();\n        Swal.fire({\n          icon: \"success\",\n          title: \"Producto Eliminado\",\n          text: \"El producto ha sido eliminado exitosamente.\",\n          confirmButtonColor: \"#0a641a\",\n        });\n      } catch (error) {\n        console.error(\n          \"Error al eliminar el producto:\",\n          error.response ? error.response.data : error.message\n        );\n        Swal.fire({\n          icon: \"error\",\n          title: \"Error\",\n          text: \"No se pudo eliminar el producto.\",\n          confirmButtonColor: \"#0a641a\",\n        });\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCategorias();\n  },\n};\n</script>\n\n<style scoped>\n.ventas-container {\n  padding: 20px;\n  background-color: #f4f4f4;\n}\n\n.ventas-title {\n  text-align: center;\n  color: #333;\n  margin-bottom: 20px;\n}\n\n.table-container {\n  margin: 0 auto;\n  max-width: 100%;\n  o