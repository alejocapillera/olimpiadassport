{"ast":null,"code":"import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      ventas: [],\n      estados: {\n        1: \"Pendiente\",\n        2: \"Enviado\",\n        3: \"Entregado\"\n        // Agregar otros estados si es necesario\n      }\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n      if (!token) {\n        alert(\"Por favor, inicie sesión para ver sus compras.\");\n        return;\n      }\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/auth/compras\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.ventas = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error);\n        alert(\"Error al obtener las compras.\");\n      }\n    },\n    obtenerEstado(id_estado) {\n      return this.estados[id_estado] || \"Desconocido\";\n    }\n  },\n  mounted() {\n    this.fetchCompras();\n  }\n};","map":{"version":3,"names":["MenuPrincipal","axios","data","ventas","estados","methods","fetchCompras","token","localStorage","getItem","alert","response","get","headers","Authorization","error","console","obtenerEstado","id_estado","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\MisCompras.vue"],"sourcesContent":["<template>\n  <div class=\"mis-compras\">\n    <h2>Mis Compras</h2>\n    <div v-if=\"ventas.length > 0\">\n      <ul>\n        <li v-for=\"venta in ventas\" :key=\"venta.id_venta\">\n          <p>ID de Venta: {{ venta.id_venta }}</p>\n          <p>Fecha: {{ venta.fecha_venta }}</p>\n          <p>Monto Total: ${{ venta.monto_total }}</p>\n          <p>Estado: {{ obtenerEstado(venta.id_estado) }}</p>\n        </li>\n      </ul>\n    </div>\n    <div v-else>\n      <p>No tienes compras registradas.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport axios from \"axios\";\n\nexport default {\n    \n  data() {\n    return {\n      ventas: [],\n      estados: {\n        1: \"Pendiente\",\n        2: \"Enviado\",\n        3: \"Entregado\",\n        // Agregar otros estados si es necesario\n      },\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para ver sus compras.\");\n        return;\n      }\n\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/auth/compras\",\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        this.ventas = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error);\n        alert(\"Error al obtener las compras.\");\n      }\n    },\n    obtenerEstado(id_estado) {\n      return this.estados[id_estado] || \"Desconocido\";\n    },\n  },\n  mounted() {\n    this.fetchCompras();\n  },\n};\n</script>\n\n<style scoped>\n.mis-compras {\n  padding: 20px;\n}\n\n.compra-card {\n  border: 1px solid #ddd;\n  border-radius: 5px;\n  padding: 15px;\n  margin-bottom: 15px;\n}\n\n.compra-card h3 {\n  margin-top: 0;\n}\n</style>\n"],"mappings":"AAoBA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE;QACP,CAAC,EAAE,WAAW;QACd,CAAC,EAAE,SAAS;QACZ,CAAC,EAAE;QACH;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAE3C,IAAI,CAACF,KAAK,EAAE;QACVG,KAAK,CAAC,gDAAgD,CAAC;QACvD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAC9B,wCAAwC,EACxC;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUP,KAAK;UAChC;QACF,CACF,CAAC;QAED,IAAI,CAACJ,MAAK,GAAIQ,QAAQ,CAACT,IAAI;MAC7B,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CA