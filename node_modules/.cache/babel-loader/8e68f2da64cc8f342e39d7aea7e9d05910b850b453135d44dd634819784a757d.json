{"ast":null,"code":"import MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    MenuPrincipal\n  },\n  data() {\n    return {\n      compras: []\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n      if (!token || !id_cliente) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n      try {\n        const response = await axios.get(`http://localhost:3001/api/auth/compras`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (Array.isArray(response.data)) {\n          this.compras = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error);\n      }\n    },\n    estadoCompra(id_estado) {\n      const estados = {\n        1: \"Pendiente\",\n        2: \"En Camino\",\n        3: \"Entregado\",\n        4: \"Cancelado\"\n      };\n      return estados[id_estado] || \"Desconocido\";\n    }\n  },\n  mounted() {\n    this.fetchCompras();\n  }\n};","map":{"version":3,"names":["MenuPrincipal","axios","components","data","compras","methods","fetchCompras","token","localStorage","getItem","id_cliente","alert","response","get","headers","Authorization","Array","isArray","console","error","estadoCompra","id_estado","estados","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\MisCompras.vue"],"sourcesContent":["<template>\n  <MenuPrincipal />\n  <div class=\"mis-compras\">\n    <h2>Mis Compras</h2>\n    <div v-if=\"compras.length > 0\">\n      <div v-for=\"compra in compras\" :key=\"compra.id_venta\" class=\"compra-card\">\n        <h3>Compra #{{ compra.id_venta }}</h3>\n        <p>Fecha: {{ compra.fecha_venta }}</p>\n        <p>Monto Total: ${{ compra.monto_total }}</p>\n        <p>Estado: {{ estadoCompra(compra.id_estado) }}</p>\n        <div v-if=\"compra.productos.length > 0\">\n          <h4>Productos:</h4>\n          <ul>\n            <li\n              v-for=\"producto in compra.productos\"\n              :key=\"producto.id_producto\"\n            >\n              {{ producto.descripcion }} - ${{ producto.precio_unitario }} x\n              {{ producto.cantidad }}\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n    <div v-else>\n      <p>No tienes compras registradas.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport MenuPrincipal from \"@/components/MenuPrincipal.vue\";\nimport axios from \"axios\";\n\nexport default {\n  components: {\n    MenuPrincipal,\n  },\n  data() {\n    return {\n      compras: [],\n    };\n  },\n  methods: {\n    async fetchCompras() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token || !id_cliente) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/auth/compras`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (Array.isArray(response.data)) {\n          this.compras = response.data;\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al obtener las compras:\", error);\n      }\n    },\n    estadoCompra(id_estado) {\n      const estados = {\n        1: \"Pendiente\",\n        2: \"En Camino\",\n        3: \"Entregado\",\n        4: \"Cancelado\",\n      };\n      return estados[id_estado] || \"Desconocido\";\n    },\n  },\n  mounted() {\n    this.fetchCompras();\n  },\n};\n</script>\n\n<style scoped>\n.mis-compras {\n  padding: 20px;\n}\n\n.compra-card {\n  border: 1px solid #ddd;\n  border-radius: 5px;\n  padding: 15px;\n  margin-bottom: 15px;\n}\n\n.compra-card h3 {\n  margin-top: 0;\n}\n</style>\n"],"mappings":"AA+BA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,MAAMC,UAAS,GAAIF,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC;MAErD,IAAI,CAACF,KAAI,IAAK,CAACG,UAAU,EAAE;QACzBC,KAAK,CAAC,0CAA0C,CAAC;QACjD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAC9B,wCAAwC,EACxC;UACEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC;QACF,CACF,CAAC;QAED,IAAIS,KAAK,CAACC,OAAO,CAACL,QAAQ,CAACT,IAAI,CAAC,EAAE;UAChC,IAAI,CAACC,OAAM,GAAIQ,QAAQ,CAACT,IAAI;QAC9B,OAAO;UACLe,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEP,QAAQ,CAACT,IAAI,CAAC;QACvD;MACF,EAAE,OAAOgB,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;MACvD;IACF,CAAC;IACDC,YAAYA,CAACC,SAAS,EAAE;MACtB,MAAMC,OAAM,GAAI;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE,WAAW;QACd,CAAC,EAAE;MACL,CAAC;MACD,OAAOA,OAAO,CAACD,SAAS,KAAK,aAAa;IAC5C;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACjB,YAAY,CAAC,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}