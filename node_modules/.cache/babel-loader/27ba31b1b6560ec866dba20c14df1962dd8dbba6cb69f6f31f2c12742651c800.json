{"ast":null,"code":"import axios from \"axios\";\nimport { BModal, BButton, BFormGroup, BFormInput, BFormSelect } from \"bootstrap-vue-3\";\nimport Swal from \"sweetalert2\";\nexport default {\n  components: {\n    BModal,\n    BButton,\n    BFormGroup,\n    BFormInput,\n    BFormSelect\n  },\n  data() {\n    return {\n      productos: [],\n      categorias: [],\n      // Lista de categorías\n      subcategorias: [],\n      // Lista de subcategorías\n      selectedCategoria: null,\n      // Categoría seleccionada en el modal de edición\n      showModalDetalles: false,\n      showModalEditar: false,\n      showModalEliminar: false,\n      productoActual: {},\n      productoAEliminar: {}\n    };\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const response = await axios.get(\"http://localhost:3001/api/listaproductos\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\"Error al obtener los productos:\", error.response ? error.response.data : error.message);\n      }\n    },\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\"http://localhost:3001/api/categorias\");\n        this.categorias = response.data.map(categoria => ({\n          value: categoria.id_categoria,\n          text: categoria.nombre_categoria\n        }));\n      } catch (error) {\n        console.error(\"Error al obtener categorías:\", error.response ? error.response.data : error.message);\n      }\n    },\n    async fetchSubcategorias() {\n      if (this.selectedCategoria) {\n        try {\n          const response = await axios.get(`http://localhost:3001/api/subcategorias/${this.selectedCategoria}`);\n          this.subcategorias = response.data.map(subcategoria => ({\n            value: subcategoria.id_subcategoria,\n            text: subcategoria.nombre_subcategoria\n          }));\n        } catch (error) {\n          console.error(\"Error al obtener subcategorías:\", error.response ? error.response.data : error.message);\n        }\n      } else {\n        this.subcategorias = []; // Limpia subcategorías si no hay categoría seleccionada\n      }\n    },\n    verDetallesProducto(producto) {\n      this.productoActual = producto;\n      this.showModalDetalles = true;\n    },\n    abrirModalEditar(producto) {\n      this.productoActual = {\n        ...producto\n      }; // Copia del producto para edición\n      this.selectedCategoria = producto.id_subcategoria; // Establece la categoría seleccionada\n      this.fetchSubcategorias(); // Obtiene las subcategorías para la categoría seleccionada\n      this.showModalEditar = true;\n    },\n    async editarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.put(`http://localhost:3001/api/productos/${this.productoActual.id_producto}`, this.productoActual, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.fetchProductos(); // Actualiza la lista de productos\n        this.showModalEditar = false;\n        Swal.fire({\n          icon: 'success',\n          title: 'Producto Editado',\n          text: 'El producto ha sido editado exitosamente.',\n          confirmButtonColor: '#0a641a'\n        });\n      } catch (error) {\n        console.error(\"Error al editar el producto:\", error.response ? error.response.data : error.message);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'No se pudo editar el producto.',\n          confirmButtonColor: '#0a641a'\n        });\n      }\n    },\n    abrirModalEliminar(producto) {\n      this.productoAEliminar = producto;\n      this.showModalEliminar = true;\n    },\n    async eliminarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(`http://localhost:3001/api/productos/${this.productoAEliminar.id_producto}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        this.fetchProductos(); // Actualiza la lista de productos\n        this.showModalEliminar = false;\n        Swal.fire({\n          icon: 'success',\n          title: 'Producto Eliminado',\n          text: 'El producto ha sido eliminado exitosamente.',\n          confirmButtonColor: '#0a641a'\n        });\n      } catch (error) {\n        console.error(\"Error al eliminar el producto:\", error.response ? error.response.data : error.message);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'No se pudo eliminar el producto.',\n          confirmButtonColor: '#0a641a'\n        });\n      }\n    }\n  },\n  watch: {\n    selectedCategorias() {\n      // Cuando cambie la categoría seleccionada, actualiza las subcategorías\n      this.fetchSubcategorias();\n    }\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCategorias(); // Obtiene las categorías al montar el componente\n  }\n};","map":{"version":3,"names":["axios","BModal","BButton","BFormGroup","BFormInput","BFormSelect","Swal","components","data","productos","categorias","subcategorias","selectedCategoria","showModalDetalles","showModalEditar","showModalEliminar","productoActual","productoAEliminar","methods","fetchProductos","token","localStorage","getItem","response","get","headers","Authorization","error","console","message","fetchCategorias","map","categoria","value","id_categoria","text","nombre_categoria","fetchSubcategorias","subcategoria","id_subcategoria","nombre_subcategoria","verDetallesProducto","producto","abrirModalEditar","editarProducto","put","id_producto","fire","icon","title","confirmButtonColor","abrirModalEliminar","eliminarProducto","delete","watch","selectedCategorias","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\ListaProductos.vue"],"sourcesContent":["<template>\n  <div class=\"productos-container\">\n    <h1 class=\"productos-title\">Lista de Productos</h1>\n    <div class=\"table-container\">\n      <table class=\"productos-table\">\n        <thead>\n          <tr>\n            <th>Descripción</th>\n            <th>Stock</th>\n            <th>Acciones</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"producto in productos\" :key=\"producto.id_producto\">\n            <td>{{ producto.descripcion }}</td>\n            <td>{{ producto.stock }}</td>\n            <td>\n              <button @click=\"verDetallesProducto(producto)\" class=\"btn-ver\">\n                Ver Detalles\n              </button>\n              <button @click=\"abrirModalEditar(producto)\" class=\"btn-editar\">\n                Editar\n              </button>\n              <button\n                @click=\"abrirModalEliminar(producto)\"\n                class=\"btn-eliminar\"\n              >\n                Eliminar\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n\n    <!-- Modal para mostrar detalles del producto -->\n    <b-modal\n      id=\"modalDetallesProducto\"\n      title=\"Detalles del Producto\"\n      v-model=\"showModalDetalles\"\n    >\n      <p><strong>Código:</strong> {{ productoActual.codigo_producto }}</p>\n      <p><strong>Descripción:</strong> {{ productoActual.descripcion }}</p>\n      <p>\n        <strong>Precio Unitario:</strong> {{ productoActual.precio_unitario }}\n      </p>\n      <p><strong>Stock:</strong> {{ productoActual.stock }}</p>\n      <p>\n        <strong>Subcategoría:</strong> {{ productoActual.nombre_subcategoria }}\n      </p>\n      <b-button @click=\"showModalDetalles = false\">Cerrar</b-button>\n    </b-modal>\n\n    <!-- Modal para editar producto -->\n    <b-modal\n      id=\"modalEditarProducto\"\n      title=\"Editar Producto\"\n      v-model=\"showModalEditar\"\n      @ok=\"editarProducto\"\n    >\n      <form @submit.prevent=\"editarProducto\">\n        <b-form-group label=\"Código\">\n          <b-form-input\n            v-model=\"productoActual.codigo_producto\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Descripción\">\n          <b-form-input\n            v-model=\"productoActual.descripcion\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Precio Unitario\">\n          <b-form-input\n            type=\"number\"\n            v-model=\"productoActual.precio_unitario\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Stock\">\n          <b-form-input\n            type=\"number\"\n            v-model=\"productoActual.stock\"\n            required\n          ></b-form-input>\n        </b-form-group>\n        <b-form-group label=\"Categoría\">\n          <b-form-select\n            v-model=\"selectedCategoria\"\n            @change=\"fetchSubcategorias\"\n            :options=\"categorias\"\n            required\n          ></b-form-select>\n        </b-form-group>\n        <b-form-group label=\"Subcategoría\">\n          <b-form-select\n            v-model=\"productoActual.id_subcategoria\"\n            :options=\"subcategorias\"\n            required\n          ></b-form-select>\n        </b-form-group>\n      </form>\n    </b-modal>\n\n    <!-- Modal de confirmación de eliminación -->\n    <b-modal\n      id=\"modalEliminarProducto\"\n      title=\"Eliminar Producto\"\n      v-model=\"showModalEliminar\"\n      @ok=\"eliminarProducto\"\n    >\n      <p>\n        ¿Estás seguro de que deseas eliminar el producto \"{{\n          productoAEliminar.descripcion\n        }}\"?\n      </p>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport {\n  BModal,\n  BButton,\n  BFormGroup,\n  BFormInput,\n  BFormSelect,\n} from \"bootstrap-vue-3\";\nimport Swal from \"sweetalert2\";\n\nexport default {\n  components: {\n    BModal,\n    BButton,\n    BFormGroup,\n    BFormInput,\n    BFormSelect,\n  },\n  data() {\n    return {\n      productos: [],\n      categorias: [], // Lista de categorías\n      subcategorias: [], // Lista de subcategorías\n      selectedCategoria: null, // Categoría seleccionada en el modal de edición\n      showModalDetalles: false,\n      showModalEditar: false,\n      showModalEliminar: false,\n      productoActual: {},\n      productoAEliminar: {},\n    };\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const response = await axios.get(\n          \"http://localhost:3001/api/listaproductos\",\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.productos = response.data;\n      } catch (error) {\n        console.error(\n          \"Error al obtener los productos:\",\n          error.response ? error.response.data : error.message\n        );\n      }\n    },\n    async fetchCategorias() {\n      try {\n        const response = await axios.get(\n          \"http://localhost:3001/api/categorias\"\n        );\n        this.categorias = response.data.map((categoria) => ({\n          value: categoria.id_categoria,\n          text: categoria.nombre_categoria,\n        }));\n      } catch (error) {\n        console.error(\n          \"Error al obtener categorías:\",\n          error.response ? error.response.data : error.message\n        );\n      }\n    },\n    async fetchSubcategorias() {\n      if (this.selectedCategoria) {\n        try {\n          const response = await axios.get(\n            `http://localhost:3001/api/subcategorias/${this.selectedCategoria}`\n          );\n          this.subcategorias = response.data.map((subcategoria) => ({\n            value: subcategoria.id_subcategoria,\n            text: subcategoria.nombre_subcategoria,\n          }));\n        } catch (error) {\n          console.error(\n            \"Error al obtener subcategorías:\",\n            error.response ? error.response.data : error.message\n          );\n        }\n      } else {\n        this.subcategorias = []; // Limpia subcategorías si no hay categoría seleccionada\n      }\n    },\n    verDetallesProducto(producto) {\n      this.productoActual = producto;\n      this.showModalDetalles = true;\n    },\n    abrirModalEditar(producto) {\n      this.productoActual = { ...producto }; // Copia del producto para edición\n      this.selectedCategoria = producto.id_subcategoria; // Establece la categoría seleccionada\n      this.fetchSubcategorias(); // Obtiene las subcategorías para la categoría seleccionada\n      this.showModalEditar = true;\n    },\n    async editarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.put(\n          `http://localhost:3001/api/productos/${this.productoActual.id_producto}`,\n          this.productoActual,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.fetchProductos(); // Actualiza la lista de productos\n        this.showModalEditar = false;\n        Swal.fire({\n          icon: 'success',\n          title: 'Producto Editado',\n          text: 'El producto ha sido editado exitosamente.',\n          confirmButtonColor: '#0a641a'\n        });\n      } catch (error) {\n        console.error(\n          \"Error al editar el producto:\",\n          error.response ? error.response.data : error.message\n        );\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'No se pudo editar el producto.',\n          confirmButtonColor: '#0a641a'\n        });\n      }\n    },\n    abrirModalEliminar(producto) {\n      this.productoAEliminar = producto;\n      this.showModalEliminar = true;\n    },\n    async eliminarProducto() {\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(\n          `http://localhost:3001/api/productos/${this.productoAEliminar.id_producto}`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n        this.fetchProductos(); // Actualiza la lista de productos\n        this.showModalEliminar = false;\n        Swal.fire({\n          icon: 'success',\n          title: 'Producto Eliminado',\n          text: 'El producto ha sido eliminado exitosamente.',\n          confirmButtonColor: '#0a641a'\n        });\n      } catch (error) {\n        console.error(\n          \"Error al eliminar el producto:\",\n          error.response ? error.response.data : error.message\n        );\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'No se pudo eliminar el producto.',\n          confirmButtonColor: '#0a641a'\n        });\n      }\n    },\n  },\n  watch: {\n    selectedCategorias() {\n      // Cuando cambie la categoría seleccionada, actualiza las subcategorías\n      this.fetchSubcategorias();\n    },\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchCategorias(); // Obtiene las categorías al montar el componente\n  },\n};\n</script>\n\n<style scoped>\n.productos-container {\n  padding: 20px;\n  background-color: #f4f4f4;\n}\n\n.productos-title {\n  text-align: center;\n  color: #333;\n  margin-bottom: 20px;\n}\n\n.table-container {\n  margin: 0 auto;\n  max-width: 100%;\n  overflow-x: auto;\n}\n\n.productos-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.productos-table th,\n.productos-table td {\n  padding: 12px;\n  border: 1px solid #ddd;\n}\n\n.productos-table th {\n  background-color: #0a641a;\n  color: #fff;\n}\n\n.productos-table td {\n  text-align: left;\n}\n\nbutton {\n  padding: 5px 10px;\n  margin: 2px;\n  border: none;\n  border-