is.carrito = response.data.carrito;\n          this.actualizarTotal();\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al cargar el carrito:\", error);\n      }\n    }\n  },\n  mounted() {\n    const id_cliente = localStorage.getItem(\"id_cliente\");\n    if (id_cliente) {\n      this.cargarCarrito();\n    } else {\n      this.$router.push({\n        name: \"home\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","carrito","totalCarrito","metodo_pago_id","methods","finalizarCompra","token","localStorage","getItem","id_cliente","alert","response","post","productos","map","p","id_producto","cantidad","precio_unitario","parseFloat","total","headers","Authorization","message","removeItem","$router","push","name","error","console","cargarCarrito","get","actualizarTotal","mounted"],"sources":["C:\\Users\\estudiante\\Desktop\\sport-tienda\\src\\components\\MiCarrito.vue"],"sourcesContent":["<template>\n  <div class=\"mi-carrito\">\n    <h2>Finalizar Compra</h2>\n\n    <!-- Sección 1: Resumen del carrito y selección de método de pago -->\n    <section class=\"section-resumen\">\n      <h3>Resumen de tu compra</h3>\n      <ul>\n        <li v-for=\"(item, index) in carrito\" :key=\"index\">\n          {{ item.descripcion }} - ${{ item.precio_unitario }} x\n          {{ item.cantidad }}\n        </li>\n      </ul>\n      <p>Total: ${{ totalCarrito }}</p>\n\n      <h3>Selecciona el método de pago</h3>\n      <b-form-select v-model=\"metodo_pago_id\">\n        <b-form-select-option value=\"1\"\n          >Tarjeta de Crédito</b-form-select-option\n        >\n        <b-form-select-option value=\"2\">Mercadopago</b-form-select-option>\n        <b-form-select-option value=\"3\">Rapipago</b-form-select-option>\n      </b-form-select>\n\n      <button @click=\"finalizarCompra\" class=\"finalizar-btn\">\n        Finalizar Compra\n      </button>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      carrito: [],\n      totalCarrito: 0,\n      metodo_pago_id: 1,\n    };\n  },\n  methods: {\n    async finalizarCompra() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      if (!token) {\n        alert(\"Por favor, inicie sesión para continuar.\");\n        return;\n      }\n\n      try {\n        const response = await axios.post(\n          \"http://localhost:3001/api/auth/venta\",\n          {\n            id_cliente: id_cliente,\n            productos: this.carrito.map((p) => ({\n              id_producto: p.id_producto,\n              cantidad: p.cantidad,\n              precio_unitario: parseFloat(p.precio_unitario),\n            })),\n            total: parseFloat(this.totalCarrito),\n            metodo_pago_id: this.metodo_pago_id,\n          },\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data.message === \"Venta registrada exitosamente\") {\n          alert(\"Compra finalizada con éxito.\");\n          localStorage.removeItem(\"carrito\");\n          localStorage.removeItem(\"totalCarrito\");\n          this.$router.push({ name: \"home\" });\n        } else {\n          alert(\"Error al finalizar la compra.\");\n        }\n      } catch (error) {\n        console.error(\"Error al finalizar la compra:\", error);\n        alert(\"Error al finalizar la compra.\");\n      }\n    },\n    async cargarCarrito() {\n      const token = localStorage.getItem(\"token\");\n      const id_cliente = localStorage.getItem(\"id_cliente\");\n\n      try {\n        const response = await axios.get(\n          `http://localhost:3001/api/auth/carrito/${id_cliente}`,\n          {\n            headers: {\n              Authorization: `Bearer ${token}`,\n            },\n          }\n        );\n\n        if (response.data && response.data.carrito) {\n          this.carrito = response.data.carrito;\n          this.actualizarTotal();\n        } else {\n          console.error(\"Respuesta inesperada:\", response.data);\n        }\n      } catch (error) {\n        console.error(\"Error al cargar el carrito:\", error);\n      }\n    },\n  },\n  mounted() {\n    const id_cliente = localStorage.getItem(\"id_cliente\");\n\n    if (id_cliente) {\n      this.cargarCarrito();\n    } else {\n      this.$router.push({ name: \"home\" });\n    }\n  },\n};\n</script>\n\n<style scoped>\n.mi-carrito {\n  max-width: 600px;\n  margin: 0 auto;\n  padding: 20px;\n  font-family: Arial, sans-serif;\n}\n\nh2 {\n  text-align: center;\n  color: #333;\n}\n\n.section-usuario,\n.section-resumen {\n  background-color: #f9f9f9;\n  border-radius: 8px;\n  padding: 20px;\n  margin-bottom: 20px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n\n.input-group {\n  margin-bottom: 15px;\n}\n\n.input-group label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\n.input-group input {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  font-size: 16px;\n}\n\n.button-group {\n  display: flex;\n  justify-content: space-between;\n}\n\n.button-group button {\n  background-color: #007bff;\n  color: white;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.button-group button:hover {\n  background-color: #0056b3;\n}\n\n.finalizar-btn {\n  background-color: #28a745;\n}\n\n.finalizar-btn:hover {\n  background-color: #218838;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  margin-bottom: 10px;\n}\n\nb-form-select {\n  width: 100%;\n  margin-bottom: 20px;\n}\n</style>\n"],"mappings":";AAgCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,CAAC;MACfC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAA